        SUBROUTINE CHECK(N1,N2,NF,IT,INT,ISTAT,IB4)
        IF(IT.EQ.2) GO TO 50
C
C       CHECK FIRST OF TWO NUMBERS
        IF(ISTAT.EQ.1) GO TO 25
        TYPE 850
 850    FORMAT(' FIRST NUMBER NOT AN INTEGER,'/
     1  ' PLEASE RE-ENTER BOTH NUMBERS: ',$)
        IB4=1
        RETURN
C
C       NUMBER IS OK
 25     N1=INT
        IB4=2
        RETURN
C
C       CHECK SECOND OF TWO NUMBERS
 50     CONTINUE
        IF(ISTAT.EQ.1) GO TO 60
        TYPE 851
 851    FORMAT(' SECOND NUMBER NOT AN INTEGER,'/
     1  ' PLEASE RE-ENTER BOTH NUMBERS: ',$)
        IB4=1
        RETURN
C
C       SECOND NUMBER IS OK
 60     CONTINUE
        N2=INT
C       FIRST NUMBER POSITIVE
        IF(N1.GT.0) GO TO 100
        TYPE 900
        TYPE 901
 900    FORMAT(/' FIRST NUMBER MUST BE STRICTLY POSITIVE')
 901    FORMAT(' PLEASE RE-ENTER TWO NUMBERS'/)
        IB4=1
        RETURN
C       
C       SECOND NUMBER POSITIVE
 100    CONTINUE
        IF(N2.GT.0) GO TO 150
        TYPE 902
        TYPE 903
 902    FORMAT(/' SECOND NUMBER MUST BE STRICTLY POSITIVE')
 903    FORMAT(' PLEASE RE-ENTER BOTH NUMBERS'/)
        IB4=1
        RETURN
C       
C       SECOND NUMBER GREATER THAN OR EQUAL TO FIRST NUMBER
 150    CONTINUE
        IF(N2.GE.N1) GO TO 175
        TYPE 904
        TYPE 905
 904    FORMAT(/' ERROR: SECOND NUMBER GREATER THAN FIRST')
 905    FORMAT(' PLEASE RE-ENTER BOTH NUMBERS')
        IB4=1
        RETURN
C       
C       SECOND NUMBER LESS THAN OR EQUAL TO MAX
 175    CONTINUE
        IF(N2.GT.NF) GO TO 200
        IB4=3
        RETURN
C       
C       CHOOSE ANOTHER FUNCTION
 200    CONTINUE
        IF(N2.NE.999) GO TO 225
        IB4=2
        RETURN
C       
C       SECOND NUMBER GREATER THAN MAX
 225    CONTINUE
        TYPE 906,NF
        TYPE 907
 906    FORMAT(/' ERROR: SECOND VALUE GREATER THAN MAX VALUE OF ',I5)
 907    FORMAT(' PLEASE RE-ENTER BOTH INPUT NUMBERS')
        IB4=1
        RETURN
        END
