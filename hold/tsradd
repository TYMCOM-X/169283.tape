PROCEDURE TSRADD
        % ADD COMMAND LEVEL - %
        % INPUT OF VALUES AND TEST VALIDITY OF EACH ENTRY %
        % CHECK FOR REDUNDANCY OF MSR NUMBER %
        % ADD TO RELATION %
        % REPEAT UNTIL NULL VALUE IN ALL FIELDS OR TSR 0 FOUND %
BEGIN
ADD1: BEGIN
        ICT=1
ADD2: BEGIN
        TYPE  ICT,":"
        ACCEPT MSR,IDATE,CLASS,SOFT,VERSION,ORIG,LOC,DESC,DESC1
        IF MSR < 1 FINISH LAB1 ELSE
        INSERT INTO MAJORDER MSR FOR MSR,IDATE FOR IDATE,
                CLASS FOR CLASS,SOFT FOR SOFT,VERSION FOR VERSION,
                ORIG FOR ORIG,LOC FOR LOC, DESC FOR DESC,DESCQ1 FOR DESC1
