TITLE SYMBOL

;ACCUMULATOR DEFINITIONS

A=1
B=2
;  0	;TEMP USED BY F4LIB
SC=1	;SCALER IN BRM IN PLOT.
TX=2	;TEMP FOR X IN PLOT.
TY=3	;TEMP FOR Y IN PLOT.
RATIO=4
CHAR=5	;CHARACTER IN SYMBOL
STROKE=6	;STROKE BYTE POINTER
TAC=6
SCT=7	;STROKE COUNT
TAC1=7
XO=10	;ORIGIN OF CHAR IN SYMBOL
YO=11	;ORIGIN OF CHAR IN SYMBOL
TP=12	;BYTE POINTER TO TEXT
TDX=13	;TEMP FOR DAT FOR X IN PLOT.
TDY=14	;TEMP FOR DAT FOR Y IN PLOT.
DAT=15	;ARGUMENT FOR PLT1,TEMP IN SYMBOL
Q=16	;JSA AC
P=17	;PUSHDOWN POINTER
	ENTRY SYMBOL
	EXTERN SIND,COSD,PLOT,WHERE
SYMBOL:	0
	MOVEM 17,SV17
	MOVEI 17,ARGS-1
	PUSH 17,@0(Q)	;SVX
	PUSH 17,@1(Q)	;SVY
	MOVEI 17,SVSZ-1
	PUSH 17,@2(Q)	;SVSZ
	PUSH 17,3(Q)	;SVTXT
	PUSH 17,@4(Q)	;SVOR
	PUSH 17,@5(Q)	;SVNTXT
	MOVEI 17,SV0
	BLT 17,SV0+16	;SAVE AC'S
	MOVE	P,SVNTXT
	MOVEM	P,SPESYM
	MOVE P,PPDP
	JSA Q,COSD
	JUMP 2,SVOR
	MOVEM 0,COSOR
	JSA Q,SIND
	JUMP 2,SVOR
	MOVEM 0,SINOR
	MOVEI TX,3
	SKIPL	TY,SPESYM
	JRST	KJ1
	MOVE	TAC,SVTXT	;FET 1ST WORD OF TEXT?
	MOVE	TAC,(TAC)
	TLNN	TAC,774000	;IS IT TEXT?
	JRST	KJ5	;NO
	SETZM	SPESYM	;CENTERED CHARATER FLAG.
	MOVEI	TAC,1
	MOVEM	TAC,SVNTXT	;SET NO OF CHAR. TO 1.
	MOVSI	TAC,(EXP .5)
	FMPR	TAC,SVSZ	;COMPUTE ADJUSTEMENT FOR CENTERING.
	MOVE	TAC1,SVY	;REDUCE Y BY 1/2 OF CHAR HEIGHT
	FSBR	TAC1,TAC	;
	MOVEM	TAC1,SVY	;AND STORE IT BACK.
	FMPRI	TAC,(EXP .75)	;ASSUME THE X-DIMENSION OF THE
	MOVE	TAC1,SVX	;GRID IS 3/4 OF THE -DIMENSION
	FSBR	TAC1,TAC	;AND ADJUST IT ACCORDINGLY.
	MOVEM	TAC1,SVX	;PUT X BACK INTO MEM.
KJ5:	CAME	TY,[-1]		;IF -1, THEN PEN UP.
	MOVEI	TX,2		;OTHERWISE PEN DOWN.
KJ1:	MOVEM TX,SVPEN	;PEN UP
	JSA Q,PLOT
	JUMP 2,SVX
	JUMP 2,SVY
	JUMP SVPEN
	SKIPG SVSZ
	JRST ENDTXT	;SIZE MUST BE >0
	MOVE TP,SVTXT
	SKIPL	SPESYM
	JRST	KJ2
	MOVE RATIO,@SVTXT	;GET THE SYMBOL NUMBER
	SETZM SVNTXT	;SO ONLY THIS ONE
	IDIVI RATIO,^D16	;GET IN RANGE
	HRR STROKE,CTBL+1(CHAR)	;BECAUSE OF 0
	HLRZ SCT,CTBL+1(CHAR)
	JRST KJ4A
KJ2:	HRLI TP,440700	;POINTER TO ASCII
LP1:	SOSGE SVNTXT
	JRST ENDTXT
KJ3:	ILDB CHAR,TP	;GET TEXT CHAR
	CAIN	CHAR,0
	MOVEI	CHAR," "

;FOLLOWING TWO LOCATIONS ARE MODIFIED TO GET DIFFERENT
;CHARACTER SETS.
T1M:	HRR STROKE,TBL1(CHAR)
T2M:	HLRZ SCT,TBL1(CHAR)	;NUMBER OF STROKES
T3M:	CAMLE CHAR,TBL1
	MOVEI SCT,0	;IGNORE IF OUT OF RANGE
KJ4A:	HRLI STROKE,440500	;SET UP FOR SYM STROKES
	JUMPE SCT,LP1	;UNDEFINED CHAR
	JSA Q,WHERE
	JUMP 2,X
	JUMP 2,Y
	MOVE XO,X
	MOVE YO,Y	;ORIGIN OF THE CHARACTER
OUT1:	ILDB TX,STROKE	;1 FOR MARK, 0 FOR BLANK
	MOVNS TX
	ADDI TX,3
	MOVEM TX,SVPEN
	ILDB TX,STROKE
	ILDB TY,STROKE
	SUBI TY,6	;OFFSET FOR VERTICAL
	SUBI	TX,6
;	SKIPE	SPESYM	;ARE WE DOING CENTERED CHARACTER?
;	JRST	KJ6	;NO
;	SUBI	TY,4	;ADJUST COORDINATES FOR CENTERED CHAR.
;	SUBI	TX,3	;
KJ6:	FSC TX,230	;FLOAT AND DIVIDE BY 8
	FSC TY,230	;"
	FMPR TX,SVSZ
	FMPR TY,SVSZ
	MOVE DAT,TX
	FMPR DAT,COSOR
	MOVN 0,TY
	FMPR 0,SINOR
	FADR 0,DAT
	FADR 0,XO
	MOVEM 0,SVX
	MOVE DAT,TY
	FMPR DAT,COSOR
	MOVE 0,TX
	FMPR 0,SINOR
	FADR 0,DAT
	FADR 0,YO
	MOVEM 0,SVY
	JSA Q,PLOT
	JUMP 2,SVX
	JUMP 2,SVY
	JUMP SVPEN
	SOJG SCT,OUT1
	JRST LP1

ENDTXT:	MOVSI 17,SV0
	BLT 17,17	;RESTORE AC'S
	JRA Q,6(Q)	;RETURN TO CALLER
;FORMAT OF CHARACTER TABLE IS: LEFT HALF, NUMBER OF LINE
;SEGMENTS IN CHARACTER. RIGHT HALF, ADDRESS OF BYTE TABLE
