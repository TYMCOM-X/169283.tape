      DIMENSION MNET(7),MDEP(7),MDTAX(7),MTOTOP(7),MEXTRA(7),MCONV(7),
     +MLTN(7),MSTB(7),MADFI(7),MSTOC(7),MTOTAL(7),MPPE(7),MDIV(7),
     +MPLONG(7),MPSHOR(7),MINV(7),MACR(7),MINVY(7),MDEXP(7),MGOOD(7),
     +MOTH(7),MTOT(7),MCASH(7),JRCD(7),JDPFD(7),JDMD(7),JTPI(7),JGCC(7),
     +JBRED(7),JFCD(7),JTDSD(7),JINT(7),JSUB(7),JXFAC(7),JTOT(7),JCSD(7)
     +,JTOTAL(7),KRCD(7),KDPFD(7),KDMD(7),KTPI(7),KGCC(7),KBRED(7),
     +KFCD(7),KTDSD(7),KOTH(7),KINT(7),KSUB(7),KXFAC(7),KTOT(7),KCSD(7),
     +LFCD(7),LTDSD(7),LTOT(7),LCSD(7),LTOTAL(7),LRCD(7),LDPFD(7),ITC(7)
     +,LGCC(7),LTPI(7),LBRED(7),LDMD(7),KTOTAL(7),ITCN(7),ITCEX(7)
      DIMENSION NINT(7),NIEX(7),NINC(7),NINEX(7),SHINC(7),SHEXC(7),
     +ICASH(8),IACR(7),IINV(7),IPRE(7),ITOCU(7),IPROP(7),IALL(7),INET(7)
     +,IADV(7),IDEXP(7),IGOOD(7),IOTH(7),ITOTAL(7),ICUR1(7),ICUR2(7),
     +ICUR3(7),ICUR4(7),ICUR5(7),ICUR6(7),ICUR(7),IDTAX(7),ILTDT(7),
     +ISTEQ(7),ILTOT(7),ITOT(7)
90      TYPE 100
100   FORMAT(1X,'ENTER BASE YEAR AND FOLLOWING 6 YEARS'/)
      ACCEPT 200,I1,I2,I3,I4,I5,I6,I7
200   FORMAT (6(A4,1X),A4)
	TYPE 1775
	ACCEPT 1780,JJJ
	IF(JJJ.EQ.3HNO ) GO TO 90
	TYPE 2095,I1
2095	FORMAT(1X,'DOES THE FILE BASE HAVE THE B.SHEET FIGS OF',A4,'?'/)
	ACCEPT 1780,JJJ
	IF(JJJ.EQ.3HN0 ) GO TO 1950
210      TYPE 300
300   FORMAT (1X'ENTER TAX RATE-EXAMPLE 53 FOR 53 PERCENT' /)
      ACCEPT 400,JT
400   FORMAT (I)
	TYPE 1775
	ACCEPT 1780,JJJ
	IF(JJJ.EQ.3HNO ) GO TO 210
410      TYPE 500
500   FORMAT (1X'ENTER NO OF SHARES' ,/)
      ACCEPT 600,NSHARE
600   FORMAT(I)
	TYPE 1775
	ACCEPT 1780,JJJ
	IF(JJJ.EQ.3HNO ) GO TO 410
      JF=I1
      I=1
650   CALL IFILE(9,JF)
      READ(9,700) MNET(I),MDEP(I),MDTAX(I),MEXTRA(I),MCONV(I),MLTN(I),
     +MSTB(I),MADFI(I),MSTOC(I),MPPE(I),MDIV(I),MPLONG(I),MPSHOR
     +(I),MINV(I),MACR(I),MINVY(I),MDEXP(I),MGOOD(I),MOTH(I),JRCD(I),
     +JDPFD(I),JDMD(I),JTPI(I),JGCC(I),JBRED(I),JFCD(I),JTDSD(I),
     +JINT(I),JXFAC(I),JCSD(I),KRCD(I),KDPFD(I),KDMD(I),KTPI(I),KGCC(I),
     +KBRED(I),KFCD(I),KTDSD(I),KOTH(I),KINT(I),KXFAC(I),KCSD(I),LRCD(I)
     +,LDPFD(I),LDMD(I),LTPI(I),LGCC(I),LBRED(I),LFCD(I),LTDSD(I),
     +LCSD(I),ITC(I)
      IF(JF.EQ.I1) GO TO 710
      IF(JF.EQ.I2) GO TO 720
      IF(JF.EQ.I3) GO TO 730
      IF(JF.EQ.I4) GO TO 740
      IF(JF.EQ.I5) GO TO 750
      IF(JF.EQ.I6) GO TO 760
      IF(JF.EQ.I7) GO TO 800
710   JF=I2
      GO TO 770
720   JF=I3
      GO TO 770
730   JF=I4
      GO TO 770
740	JF=I5
      GO TO 770
750   JF=I6
      GO TO 770
760   JF=I7
770   I=I+1
      GO TO 650
800   I8=4HBASE
      CALL IFILE(9,I8)
      READ(9,900) ICASH(1),ICASH(2),IACR(1),IINV(1),IPRE(1),IPROP(1),
     +IALL(1),IADV(1),IDEXP(1),IGOOD(1),IOTH(1),
     +ICUR1(1),ICUR2(1),ICUR3(1),ICUR4(1),ICUR5(1),ICUR6(1),IDTAX(1),
     +ILTDT(1),ISTEQ(1)
900   FORMAT(16X,I7,30X)
700   FORMAT(16X,I6,30X)
C     COMPUTATIONS BEGIN
C COMPUTATION OF TOTALS AND SUBTOTALS FOR
C THE S&A,SALES,OPERATING INC AND CORP STATEMENTS
      DO 920 I=1,7
      MTOTOP(I)=MNET(I)+MDEP(I)+MDTAX(I)
      MTOTAL(I)=MTOTOP(I)+MEXTRA(I)+MCONV(I)+MLTN(I)+MSTB(I)+MADFI(I)+
     +MSTOC(I)
      MTOT(I)=MPPE(I)+MDIV(I)+MPLONG(I)+MPSHOR(I)+MINV(I)+MACR(I)+
     +MINVY(I)+MOTH(I)+MGOOD(I)+MDEXP(I)
      MCASH(I)=MTOTAL(I)-MTOT(I)
      JSUB(I)=JRCD(I)+JDPFD(I)+JDMD(I)+JTPI(I)+JGCC(I)+JBRED(I)+
     +JFCD(I)+JTDSD(I)+JINT(I)
      JTOT(I)=JSUB(I)+JXFAC(I)
      JTOTAL(I)=JTOT(I)+JCSD(I)
      KSUB(I)=KRCD(I)+KDPFD(I)+KDMD(I)+KTPI(I)+KGCC(I)+KBRED(I)+
     +KFCD(I)+KTDSD(I)+KINT(I)+KOTH(I)
	KTOT(I)=KSUB(I)+KXFAC(I)
      KTOTAL(I)=KTOT(I)+KCSD(I)
      LTOT(I)=LRCD(I)+LDPFD(I)+LDMD(I)+LTPI(I)+LGCC(I)+LBRED(I)+
     +LFCD(I)+LTDSD(I)
      LTOTAL(I)=LTOT(I)+LCSD(I)
C     COMPUTATIONS FOR FINANCIAL STATEMENT
      NINT(I)=KTOTAL(I)-LTOTAL(I)
      NIEX(I)=KTOT(I)-LTOT(I)
	AJDUM1=(NINT(I)*(100-JT))/100
	NINC(I)=INT(AJDUM1)
	AJDUM2=(NIEX(I)*(100-JT))/100
	NINEX(I)=INT(AJDUM2)
	ITCN(I)=NINC(I)+ITC(I)
	ITCEX(I)=NINEX(I)+ITC(I)
	AJDUM3=(ITCN(I)*100000/NSHARE)
	SHINC(I)=INT(AJDUM3)/100
	AJDUM4=(ITCEX(I)*100000/NSHARE)
	SHEXC(I)=INT(AJDUM4)/100
920	CONTINUE
C     UPDATING BALANCE SHEET
      DO 960  I=1,6
      ICASH(I+2)=ICASH(I+1)+MCASH(I+1)
      IACR(I+1)=IACR(I)+MACR(I+1)
      IINV(I+1)=IINV(I)+MINVY(I+1)
      IPROP(I+1)=IPROP(I)+MPPE(I+1)
	IPRE(I+1)=IPRE(I)
      IALL(I+1)=IALL(I)+MDEP(I+1)
      IADV(I+1)=IADV(I)+MINV(I+1)
      IDEXP(I+1)=IDEXP(I)+MDEXP(I+1)
      IGOOD(I+1)=IGOOD(I)+MGOOD(I+1)
      IOTH(I+1)=IOTH(I)+MOTH(I+1)
      ICUR1(I+1)=ICUR1(I)+MSTB(I+1)-MPSHOR(I+1)
      ICUR2(I+1)=ICUR2(I)
      ICUR3(I+1)=ICUR3(I)
      ICUR4(I+1)=ICUR4(I)
      ICUR5(I+1)=ICUR5(I)
      ICUR6(I+1)=ICUR6(I)
      IDTAX(I+1)=IDTAX(I)+MDTAX(I+1)
      ILTDT(I+1)=ILTDT(I)+MLTN(I+1)-MPLONG(I+1)+MADFI(I+1)+MCONV(I+1)
	ISTEQ(I+1)=ISTEQ(I)+MNET(I+1)+MEXTRA(I+1)+
     +	MSTOC(I+1)-MDIV(I+1)
960	CONTINUE
      DO 950  I=1,7
      ITOCU(I)=ICASH(I+1)+IACR(I)+IINV(I)+IPRE(I)
      INET(I)=IPROP(I)-IALL(I)
      ITOT(I)=ITOCU(I)+INET(I)+IADV(I)+IDEXP(I)+IGOOD(I)+IOTH(I)
      ICUR(I)=ICUR1(I)+ICUR2(I)+ICUR3(I)+ICUR4(I)+ICUR5(I)+ICUR6(I)
      ITOTAL(I)=ICUR(I)+IDTAX(I)+ILTDT(I)+ISTEQ(I)
  950 CONTINUE
2500	TYPE 1790
1790	FORMAT(//1X,'WHICH STATEMENT DO YOU WANT?TYPE CODE FROM BELOW'/)
	TYPE 1795
1795	FORMAT(1X,'1=SOUR&APPLN  2=SALES  3=OPERATG INC  4=COR SER'/)
	TYPE 1800
1800	FORMAT(1X,'5=COMP FIN STMT  6=BAL SHEET  0=ALL STMS  7=N0NE'/)
	ACCEPT 1810,JLJ
1810	FORMAT(I)
	IF(JLJ.GT.7) GO TO 1925
	IF(JLJ.EQ.7) GO TO 1950
