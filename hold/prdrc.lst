   309	LOCAL BUFF(0:3), PB, P, N, %WRITE.TMP
   310	
   311	IF SFOED THEN DO
   312	   PB_CHPT(BUFF,-1) ; P_CHPT('CHANGE',-1) ; N_0
   313	   WHILE NCHV PB_NCHV P#EOLIT THEN INC N
   314	   CHV PB_CARRET ; NCHV PB_LNFEED ; NCHV PB_EOLIT
   315	   WRITE.TMP('SFC',BUFF,((N+2)/CPW+1))
   316	   SFRUN
   317	END ELSE EXU(OCT 47,0,OCT 12,0)
