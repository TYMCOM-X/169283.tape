   286	%ASK (QUES,ANS())       ! Ask 'QUES' and check for possible 'ANS'
   287	LOCAL MATCH,I,PL,PC,N,MATN,LINE
   288	
   289	MSG(QUES)       ! Type question
   290	LOOP DO
   291	 LINE _ GETLINE(TEL)    ! Read answer from terminal
   292	 MATCH _ 0
   293	 FOR I _ 1 TO ANS(0) DO
   294	  PC _ CHPT(ANS(I),-1)
   295	  PL _ LINE
   296	  N _ 0
   297	  WHILE NCHV PL=NCHV PC THEN INC N
   298	  IF (CHV PL)=CARRET AND N>0 &
   299	   THEN IF NOT MATCH OR N>MATN THEN [MATCH _ I ; MATN _ N] &
   300	   ELSE IF N=MATN THEN MATCH _ 0
   301	 END
   302	 IF MATCH THEN RETURN MATCH
   303	 MSG('WHAT?? ')
   304	END
   305	
   306	END ASK
