   349	WHILE NCHV P_NCHV I#EOLIT THEN NULL
   350	P_CHPT(FILE,-1)
   351	SETFID(FID.TBL,@P,TMP.EXT,TMPERR)
   352	T.BLK(0)_NAMARG(FID.TBL) SHL 18
   353	T.BLK(1)_IOWD(BUFF,N)
   354	RETURN
   355	END GET.NAME
