	\

<scan-THEN>   ::=
	THEN  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-THEN>  \
	\

<end-fld>   ::=
	<end-stmt> <default-fld>  ! UP 1 !  \
	\

<case-BEGIN>   ::=
	BEGIN  ! UP 1 !  \
	<new-lit> <CREATE>  ! BEGIN UP 1 !  \
	<new-stmt> <CREATE>  ! BEGIN UP 1 !  \
	<ANY>  ! NOBACKUP !  <case-BEGIN>  ! UP 1 !  \
	\

<case-colon>   ::=
	:  \
	<new-lit>  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <case-colon>  \
	\

<cases-err>   ::=
	END  ! RESCAN !  \
	<new-lit> <case-list>  ! UP 1 !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <cases-err>  \
	\

<cases-to-END>   ::=
	END  \
	<new-lit> <case-list> END  ! ERROR 100 <cases-to-END> !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <cases-to-END>  \
	\

<new-lit>   ::=
	<quoted-string>  ! RESCAN !  \
	<@integer>  ! RESCAN !  \
	@CR  ! RESCAN !  \
	DATE  ! RESCAN !  \
	<integer>  ! RESCAN !  \
	<dec-number>  ! RESCAN !  \
	+  ! RESCAN !  \
	-  ! RESCAN !  \
	\

<end-new>   ::=
	<end-stmt> <new-name>  ! UP 1 !  \
	\

<src-dec-err>   ::=
	<scan-src-dec> <new-name>  ! UP 1 !  \
	\

