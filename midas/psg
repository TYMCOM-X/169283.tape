	)  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <short-list-err>  ! UP 1 !  \
	\

<list-fld-err>   ::=
	<scan-paren-list> <default-fld>  ! UP 1 !  \
	\

<list-ae-err>   ::=
	<scan-list> <ae-1>  ! UP 1 !  \
	\

<scan-FOR-EACH>   ::=
	,  ! RESCAN !  \    &! <sort-item>
	WRITE  ! RESCAN !  \
	CONTROLLED  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-FOR-EACH>  \
	\

<REPORTS-err>   ::=
	CONTROLLED  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <REPORTS-err>  \
	\

