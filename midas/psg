	<scan-FOR-EACH> <default-qrel>  ! UP 1 !  \
	\

<scan-paren-list>   ::=
	,  ! RESCAN !  \
	)  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-paren-list>  \
	\

<SELECT-qrel>   ::=
