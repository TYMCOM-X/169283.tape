	FROM  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-src-dec>  \
	\

<src-FROM-err>   ::=
	FROM  \
	INVALIDS  ! RESCAN !  \
	NO  ! RESCAN !  \
	FIXED  ! RESCAN !  \
	FREE  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <src-FROM-err>  \
	\

<src-file-err>   ::=
	<scan-source> <default-file>  ! UP 1 !  \
	\

<default-file>   ::=
	<CREATE>  ! <quoted-string> SUBID TV$7FIL UP 1 CALL $BETOK !  \
	\

<scan-source>   ::=
	INVALIDS  ! RESCAN !  \
	NO  ! RESCAN !  \
	FIXED  ! RESCAN !  \
	FREE  ! RESCAN !  \
	SEPARATOR  ! RESCAN !  \
	TERMINATOR  ! RESCAN !  \
	LINE.LENGTH  ! RESCAN !  \
	INPUT  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-source>  \
	\

<source-NULL>   ::=
	<scan-source> <NULL>  ! UP 1 !  \
	\

<source-FREE>   ::=
	<scan-source> <get-descr>  ! UP 1 !  \
	\

<get-descr>   ::=
	FIXED <scan-source>  ! UP 2 !  \
	FREE <scan-source>  ! UP 2 !  \
	<NULL> <CREATE>  ! FREE UP 1 !  \
	\

<source-ae>   ::=
	<scan-source> <ae-1>  ! UP 1 !  \
	\

<INPUT-err>   ::=
	INPUT \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <INPUT-err> \
	\

