PRODUCTIONS


<procedure>   ::=
	PROCEDURE  ! ERROR 100 <PROC-err> !
	  <procedure-name>  ! ERROR 101 <proc-name-err> !
	  ( <formal-param-list>
	  )  ! ERROR 100 <rparen-err> UP 4 5 2 CALL $BPRCP !
	  <procedure-stmt>  ! ERROR 101 <stmt-err> !
	  <eof>  ! ERROR 102 <stmts-to-eof> CALL $BFINI !  \
	PROCEDURE  ! ERROR 100 <PROC-err> !
	  <procedure-name>  ! ERROR 101 <proc-name-err> !
	  <NULL>  ! UP 2 3 CALL $BPRCP !   &! COMMONALITY
	  <procedure-stmt>  ! ERROR 101 <stmt-err> !
	  <eof>  ! ERROR 102 <stmts-to-eof> CALL $BFINI !  \
	\

&! TO ISOLATE RESCAN WHICH ALWAYS MUST ACCOMPANY <eof-term>
<eof>   ! NV$EOF !   ::=
	<eof-term>  ! UP 1 RESCAN !  \
	\

<procedure-name>   ! NV$PRCNAM !   ::=
	<name>  ! UP 1 !  \
	\

<formal-param-list>   ::=
	<formal-param>  ! UP 1 NOBACKUP !
 	  , <formal-param-list>  ! APPEND 1 !  \
	<formal-param>  ! UP 1 NOBACKUP !  \
	\

<formal-param>   ! NV$PARAM !   ::=
	<field-name>  ! ERROR 101 <fld-dec-err> !
	  <field-attributes>  ! NEW 2 1 !  \
	<field-name>  ! ERROR 101 <fld-dec-err> !
	  <NULL>  ! NEW 2 !  \  &! COMMONALITY
	\

<procedure-stmt>   ! NV$STMT !   ::=
	BEGIN <decl-list>  ! UP 2 CALL $BWTRE !
	  <stmt-list-option>
	  END  ! ERROR 103 <stmts-to-END> CALL $BWTRE !  \
	<stmt>  \
	\

<decl-list>   ::=
	<decl> <decl-list>  \
	<decl>  \
	\

<decl>   ::=

&! Keywords extracted for jump vector effect.

	FIELD  ! NOBACKUP !  <FIELD-stmt>  \
&!	FIELDS  ! NOBACKUP !  <FIELD-stmt>  \
	SOURCE  ! NOBACKUP !  <SOURCE-stmt> \
	REPORT  ! NOBACKUP !  <REPORT-stmt> \
	FORMAT  ! NOBACKUP !  <FORMAT-stmt> \
	RELATION  ! NOBACKUP !  <RELATION-stmt> \
	\

<stmt>   ! NV$STMT !   ::=
	<stmt-1>  ! UP 1 CALL $BSTMT !  \
	\

<stmt-1>   ::=
	DELETE  ! NOBACKUP !  <DELETE-stmt> <CREATE>  ! BGDEL UP 1 2 3 !  \
	INSERT  ! NOBACKUP !  <INSERT-stmt> <CREATE>  ! BGINS UP 1 2 3 !  \
	ALTER  ! NOBACKUP !  <ALTER-stmt> <CREATE>  ! BGALT UP 1 2 3 !  \
	MOVE  ! NOBACKUP !  <MOVE-stmt> <CREATE>  ! LET !
	  <CREATE>  ! BGASS UP 1 3 2 !  \
	LET  ! NOBACKUP !  <LET-stmt> <CREATE>  ! BGASS UP 1 2 3 !  \
	ADD  ! NOBACKUP !  <ADD-stmt> <CREATE>  ! BGADD UP 1 2 3 !  \
	SUBTRACT  ! NOBACKUP !  <SUBTRACT-stmt> <CREATE>  ! BGSUB UP 1 2 3 ! \
	ACCEPT  ! NOBACKUP !  <ACCEPT-stmt> <CREATE>  ! BGACC UP 1 2 3 ! \
	TYPE  ! NOBACKUP !  <TYPE-stmt> <CREATE>  ! BGTYPE UP 1 2 3 !  \
	PRINT  ! NOBACKUP !  <PRINT-stmt> <CREATE>  ! BGPRNT UP 1 2 3 !  \
	FOR  ! NOBACKUP !  <FOR-EACH-stmt> <CREATE>  ! NDFORE UP 2 1 !  \
	WRITE  ! NOBACKUP !
	  <CREATE>  ! BGWRPT CALL $BWTRE !
	  <WRITE-REPORT-stmt> <CREATE>  ! NDWRPT UP 3 2 1 !  \
	SELECT  ! RESCAN NOBACKUP !
	  <SELECT-stmt> <CREATE>  ! NDSEL UP 2 1 !  \
	FORCE  ! NOBACKUP !  <FORCE-stmt> <CREATE>  ! BGFRCE UP 1 2 3 !  \
	NOTHING  ! UP 1 !  \
	REPEAT  ! NOBACKUP !  <REPEAT-stmt> <CREATE>  ! BGRPET UP 1 2 3 !  \
	FINISH  ! NOBACKUP !  <FINISH-stmt> <CREATE>  ! BGFIN UP 1 2 3 !  \
	IF  ! RESCAN NOBACKUP !
	  <IF-stmt> <CREATE>  ! NDIF UP 2 1 !  \
	ABORT  ! NOBACKUP !  <ABORT-stmt> <CREATE>  ! BGABT UP 1 2 3 !  \
	DO  ! NOBACKUP !  <DO-WHILE-stmt> <CREATE>  ! NDDOWL UP 2 1 !  \
	WHILE  ! NOBACKUP !  <WHILE-DO-stmt> <CREATE>  ! NDWLDO UP 2 1 !  \
	BEGIN  ! NOBACKUP !   ! CALL $BWTRE !
	  <stmt-list-option>
	  END  ! ERROR 103 <stmts-to-END> UP 3 2 1 !  \
	<labelled-stmt>  ! UP 1 !  \
	\

&! <new-stmt> must include every terminal that can follow a <stmt>.
<new-stmt>   ::=
	DELETE  ! RESCAN !  \
	INSERT  ! RESCAN !  \
	ALTER  ! RESCAN !  \
	MOVE  ! RESCAN !  \
	LET  ! RESCAN !  \
	ADD  ! RESCAN !  \
	SUBTRACT  ! RESCAN !  \
	ACCEPT  ! RESCAN !  \
	TYPE  ! RESCAN !  \
	PRINT  ! RESCAN !  \
	FOR  ! RESCAN !  \
	WRITE  ! RESCAN !  \
	SELECT  ! RESCAN !  \
	FORCE  ! RESCAN !  \
	NOTHING  ! RESCAN !  \
	REPEAT  ! RESCAN !  \
	FINISH  ! RESCAN !  \
	IF  ! RESCAN !  \
	ABORT  ! RESCAN !  \
	DO  ! RESCAN !  \
	WHILE  ! RESCAN !  \
	BEGIN  ! RESCAN !  \
	FIELD  ! RESCAN !  \
	SOURCE  ! RESCAN !  \
	REPORT  ! RESCAN !  \
	FORMAT  ! RESCAN !  \
	RELATION  ! RESCAN !  \
	<label>  ! RESCAN !  \
	END  ! RESCAN !  \
	<eof>  \
	ELSE  ! RESCAN !  \
	REPORT.HEADING  ! RESCAN !  \
	REPORT.FOOTING  ! RESCAN !  \
	PAGE.HEADING  ! RESCAN !  \
	PAGE.FOOTING  ! RESCAN !  \
	GROUP.HEADING  ! RESCAN !  \
	GROUP.FOOTING  ! RESCAN !  \
	\

<stmt-list-option>   ::=
	<stmt-list>  ! UP 1 !  \
	<NULL> \
	\

<stmt-list>   ::=
	<stmt>  ! UP 1 !
	  <stmt-list>  ! APPEND 1 !  \
	<stmt>  ! UP 1 !  \
	\


&! BEGIN <schema-decl>   (<FIELD-stmt> and <RELATION-stmt>)

<FIELD-stmt>  ::=
	<field-def-list>   \
	\

<field-def-list>   ::=
	<field-def> , <field-def-list>  \
	<field-def>  \
	\

<field-def>   ::=
	<field-name>  ! ERROR 101 <fld-dec-err> !
	  <field-attributes>  ! ERROR 101 <attr-err2 1 CALL $BFLDP !  \
	\

<field-attributes>   ! NV$DATATR !   ::=
	SAME AS  ! ERROR 100 <scan-attr> !
	  <field-name>  ! ERROR 101 <attr-fld> !
	  <value-constraints> <default-value>
	    ! NEW 3 2 1 ALT NV$SAME !  \
	AS <picture>  ! ERROR 101 <attr-pic> !
	  <value-constraints> <default-value>
	  ! NEW 3 2 1 ALT NV$IS !  \
	\

&! BACKUPOK because of <arith-exp>
<picture>   ! NV$PIC !   ::=
	<data-picture>  ! UP 1 !  \
	DATE <date-picture>  ! UP 1 2 !  \
	DATE <CREATE>  ! <quoted-string> SUBID TV$7DAT UP 1 2
	  CALL $BETOK BACKUPOK !  \
	\

<value-constraints>   ::=
	VERIFY <cond-exp>  ! ERROR 101 <attr-NULL> UP 1 2 !  \
	<NULL-constraints>  ! UP 1 !  \
	\

<NULL-constraints>   ::=
	<NULL> <CREATE>  ! VERIFY UP 2 1 !  \
	\

<default-value>   ::=
	DEFAULT  
	  <literal>  ! ERROR 101 <attr-NULL> UP 1 !  \
	<NULL>  ! UP 1 !  \
	\

<data-picture>   ::=
	<quoted-string> <CREATE>  ! PIC UP 2 1 !  \
	\

<date-picture>   ::=
	<quoted-string>  ! UP 1 !  \
	\

<RELATION-stmt>   ::=
	<relation-name>  ! ERROR 101 <rel-dec-err> !
	  <rel-def>  ! ERROR 101 <rel-def-err> UP 2 1 CALL $BRELP !  \
	\

<rel-def>   ! NV$RELDEF !   ::=
	SAME AS   ! ERROR 100 <end-stmt> !
	  <relation-name>  ! ERROR 101 <end-rel> NEW 1 ALT NV$SAME !  \
	IS <key-list>
	  <data-list>  ! NEW 2 1 ALT NV$IS !  \
	\

<relation-name>   ! NV$RELNAM !   ::=
	<name>  ! UP 1 !  \
	\

<key-list>   ::=
	KEY  ! ERROR 100 <KEY-err> !
	  <field-name-elist>  ! NEW 1 !  \
	\

<data-list>   ::=
	DATA <field-name-elist>  ! NEW 1 !  \
	<NULL>  ! NEW !  \
	\

<field-name-elist>   ::=
	<field-name>  ! ERROR 101 <scan-list> UP 1 !
	  , <field-name-elist>  ! APPEND 1 !  \
	<field-name>  ! ERROR 101 <scan-list> UP 1 !  \
	\

<field-name>   ! NV$FLDNAM !   ::=
	<name>  ! UP 1 !  \
	\

&! END <schema-decl>


<DELETE-stmt>   ::=

&! Semantic analyzer will detect if qualification not null when
&! ALL is specified.

	FROM  ! ERROR 100 <scan-edit-stmt> !
	  <qualified-relation>  ! ERROR 101 <edit-rel-err> !
	  <ALL-option>  ! UP 2 1 !  \
	\

<ALL-option>   ::=
	ALL  ! UP 1 !  \
	<NULL>  \
	\

<INSERT-stmt>   ::=
	INTO   ! ERROR 100 <scan-edit-stmt> !
	  <relation-name>  ! ERROR 101 <edit-rel-err> !
	  <using-clause>   ! UP 2 1 !  \
	\

<ALTER-stmt>   ::=
	<relation-name>  ! ERROR 101 <edit-rel-err> !
	  <using-clause>   ! UP 2 1 !  \
	\

<using-clause>   ::=
	 USING (  ! ERROR 100 <stmt-nparen> !
	  <using-list> )  ! ERROR 100 <rparen-err> UP 2 !  \
	<NULL> <CREATE>  ! USING UP 1 !  \
	\

<using-list>   ::=
	<short-list> <CREATE>  ! USNGS UP 2 1 !  \
	<long-list>  ! CALL $BDPCT !
	  <CREATE>  ! USNGL UP 2 1 ! \
	\

<short-list>   ::=
	<ident-ref> FOR <field-name>  ! ERROR 101 <list-fld-err> UP 3 1 2 !
	  , <short-list>  ! ERROR 101 <short-list-err> APPEND 1 !  \
	<ident-ref> FOR <field-name>  ! ERROR 101 <list-fld-err> UP 3 1 2 !  \
	\

<long-list>   ::=
	<ident-ref-new>  ! UP 1 NOBACKUP !
	  , <long-list>  ! APPEND 1 !  \
	<ident-ref-new>  ! UP 1 NOBACKUP !  \
	\

<ident-ref-new>   ::=
	<ident-ref>  ! ERROR 101 <list-fld-err> NEW 1 ! \
	\

<FOR-EACH-stmt>   ::=
	EACH  ! ERROR 100 <scan-FOR-EACH> !
	  <CREATE>  ! BGFORE !
	  <source>  ! ERROR 101 <EACH-qrel> !
	  <report-clause> <control-clause>
	  <CREATE>  ! EACH UP 5 4 3 2 1 CALL $BWTRE !
	  <stmt>  ! ERROR 101 <stmt-err> !  \   &! DISALLOWED IN <report-cond-def>
	\

<source>   ! NV$SRC !   ::=
	<qualified-relation> SORTED ON  
	  <sort-elist>  ! CALL $BDPCT !
	  <CREATE>  ! BGSRT !
	  <CREATE>  ! SORTEDON UP 6 2 3 1 !  \
	<qualified-relation> SORTED
 	  <CREATE>  ! SORTED UP 3 1 !  \    &! COMMONALITY
	<qualified-relation> <CREATE>  ! SORTED UP 2 1 CALL $BSRCN !  \
	\

<report-clause>   ::=
	WRITE REPORT <report-spec>  ! UP 1 !  \
	WRITE REPORTS  ! ERROR 100 <REPORTS-err> !
	  <report-spec>  ! UP 1 !  \
	<NULL>  \
	\

<report-spec>   ::=
	<report-name-list>  ! CALL $BDPCT !
	  <CREATE>  ! WRPT UP 2 1 !  \
	<NULL> <CREATE>  ! WRPT UP 1 !  \
	\

<report-name-list>   ::=
	<report-name-new>  ! UP 1 !
	  , <report-name-list>  ! APPEND 1 !  \
	<report-name-new>  ! UP 1 !  \
	\

<report-name-new>   ::=
	<name>  ! NEW 1 !  \
	\

<control-clause>   ::=
	CONTROLLED BY  ! ERROR 100 <end-stmt> !
          <cond-name-elist>  ! CALL $BDPCT !
	  <NULL>  ! UP 2 4 !  \
	<NULL>  \
	\

<cond-name-elist>   ::=
	<cond-name-new>  ! ERROR 101 <scan-list> UP 1 !
          , <cond-name-elist>  ! APPEND 1 !  \
	<cond-name-new>  ! ERROR 101 <scan-list> UP 1 !  \
	\

<cond-name-new>   ! NV$CNDNAM !   ::=
	<name>  ! NEW 1 !  \
	\

<sort-elist>   ::=
	<sort-item>  ! ERROR 101 <scan-FOR-EACH> UP 1 !
	  , <sort-elist>  ! APPEND 1 !  \
	<sort-item>  ! ERROR 101 <scan-FOR-EACH> UP 1 !  \
	\

<sort-item>   ! NV$SRTITM !   ::=
	<order> <field-name>  ! NEW 1 2 !  \
	\

<order>   ::=
	<ascending>  ! UP 1 !  \
	<descending>  ! UP 1 !  \
	<NULL> <CREATE>  ! ASCENDING UP 1 !  \
	\

<ascending>   ::=
	ASCENDING  ! UP 1 !  \
	+ <CREATE>  ! ASCENDING UP 1 !  \
	\

<descending>   ::=
	DESCENDING  ! UP 1 !  \
	- <CREATE>  ! DESCENDING UP 1 !  \
	\

<WRITE-REPORT-stmt>   ::=
	REPORT <report-spec> !  CALL $BWTRE !
	  <stmt>  ! ERROR 101 <stmt-err> !  \
	REPORTS  ! ERROR 100 <REPORTS-err>  !
	  <report-spec> !  CALL $BWTRE !
	  <stmt>  ! ERROR 101 <stmt-err> !  \
	\

<SELECT-stmt>   ::=
	SELECT <via-qualified-relation>  ! ERROR 101 <SELECT-qrel> !
	  <CREATE>  ! BGSEL UP 1 2 3 CALL $BWTRE !
	  <then-clause>  ! APPEND 1 !  \
	\

<input-elist>   ::=
	<input-elist-1> <CREATE>  ! NDILST UP 2 1 ! \
	\

<output-list>   ::=
	<output-list-1> <CREATE>  ! NDOLST UP 2 1 ! \
	\

<output-elist>   ::=
	<output-elist-1> <CREATE>  ! NDOLST UP 2 1 !  \
	\

<input-elist-1>   ::=
	<input-item>  ! ERROR 101 <scan-list> UP 1 !
	  , <input-elist-1>  ! APPEND 1 !  \
	<input-item>  ! ERROR 101 <scan-list> UP 1 !  \
	\

<input-item>   ! NV$INPITM !   ::=
	CONDITIONAL ON  ! ERROR 100 <end-stmt> !
	  <field-name>  ! ERROR 101 <end-fld> !
	  BEGIN  ! ERROR 100 <case-BEGIN> !
	  <case-list>  ! ERROR 101 <cases-err> !
	  END  ! ERROR 100 <cases-to-END>  !
	  <CREATE>  ! BGCOND UP 1 5 3 7 !  \
	@CR  ! UP 1 !  \
	<field-name>  ! UP 1 SUBID TV$INPUT !
	  <format-picture>  ! APPEND 1 !  \
        <field-name>  ! UP 1 SUBID TV$INPUT !  \
	<IO-item>  ! UP 1 !  \
	\

<case-list>   ! NV$CASLST !   ::=
	<case-item>  ! UP 1 !
	  <case-list>  ! APPEND 1 !  \
	<case-item>  ! UP 1 !  \
	\

<case-item>   ::=
	<literal> :  ! ERROR 100 <case-colon> !
	  <input-elist>  ! UP 3 1 !  \
	\

<output-list-1>   ::=
	<output-item>  ! UP 1 NOBACKUP !
	  , <output-list-1>  ! APPEND 1 !  \
	<output-item>  ! UP 1 NOBACKUP !  \
	\

<output-elist-1>   ::=
	<output-item>  ! ERROR 101 <scan-list> UP 1 !
	  , <output-elist-1>  ! APPEND 1 !  \
	<output-item>  ! ERROR 101 <scan-list> UP 1 !  \
	\

<output-item>   ! NV$OUTITM !   ::=
	@SKIP TO <arith-exp>  ! ERROR 101 <list-ae-err> !
          <CREATE>  ! SKIPTO UP 2 1 !  \
	@SKIP <arith-exp>  ! ERROR 101 <list-ae-err> !
	  <CREATE>  ! @SKIP UP 2 1 !  \    &! COMMONALITY
	<TRIM-function>  ! UP 1 !  \
	<IO-item>  ! UP 1 !  \
        <arith-exp>  ! UP 1 CALL $BFRMN !  \
	\

<IO-item>   ::=
	@TAB TO <arith-exp>  ! ERROR 101 <list-ae-err> !
          <CREATE>  ! TABTO UP 2 1 !  \
	@TAB <arith-exp>  ! ERROR 101 <list-ae-err> !
          <CREATE>  ! @TAB UP 2 1 !  \    &! COMMONALITY
	\

<FORMAT-stmt>   ::=
	<format-name>  ! ERROR 101 <end-new> !
	  <output-elist-1>  ! UP 2 1 CALL $BFRMP !  \
	\

<format-name>   ! NV$FRMNAM !   ::=
	<name>  ! UP 1 !  \
	\

<SOURCE-stmt>   ::=
	<source-name>  ! ERROR 101 <src-dec-err> !
	  FROM  ! ERROR 100 <src-FROM-err> !
	  <file>  ! ERROR 101 <src-file-err> !
	  <invalids-clause> <no-verify-clause>
	  <source-descr>  ! ERROR 101 <source-FREE> !
	  <source-param-option>
	  INPUT   ! ERROR 100 <INPUT-err> !
	  <input-elist>  ! UP 9 7 4 6 5 3 1 CALL $BSRCP !  \
	\

<source-name>   ! NV$SRCNAM !   ::=
	<name>  ! UP 1 ! \
	\

<source-descr>   ! NV$SDESCR !   ::=
	FIXED FORM  ! ERROR 100 <scan-source> UP 2 !  \
	FREE FORM  ! ERROR 100 <scan-source> UP 2 !  \
	\

<invalids-clause>   ::=
	INVALIDS TO  ! ERROR 100 <scan-source> !
	  <file>  ! ERROR 101 <source-NULL> UP 1 !  \
	<NULL>  ! UP 1 !  \
	\

<no-verify-clause>   ::=
	NO VERIFY  ! ERROR 100 <scan-source> UP 2 !  \
	<NULL>  ! UP 1 !  \
	\

<source-param-option>   ::=
	<source-param-list>  ! NEW 1 !  \
	<NULL>  ! NEW !  \
	\

<source-param-list>   ::=
	<source-param>  ! UP 1 NOBACKUP !
	  <source-param-list>  ! APPEND 1 !  \
	<source-param>  ! UP 1 NOBACKUP !  \
	\

<source-param>   ::=
	<field-delim>  ! NEW 1 ALT NV$SEPAR !  \
	<record-delim>  ! NEW 1 ALT NV$TERMIN !  \
	<line-limit>  ! NEW 1 ALT NV$LINE !  \
	\

<field-delim>  ::=
	SEPARATOR <literal>  ! ERROR 101 <source-NULL> UP 1 !  \
	\

<record-delim>   ::=
	TERMINATOR <literal>  ! ERROR 101 <source-NULL> UP 1 !  \
	\

<line-limit>   ::=
	LINE.LENGTH <arith-exp>  ! ERROR 101 <source-ae> UP 1 2 !  \
	\

<REPORT-stmt>   ::=
	<REPORT-stmt-1>
	  <report-param-option>  ! CALL $BRPCP !
	  <report-cond-def-option>  ! UP 3 2 1 CALL $BRPTP !  \
	\

<REPORT-stmt-1>   ::=
	<report-name-option> <append-option>
	  TO  ! ERROR 100 <rpt-TO-err> !
	  <file>  ! ERROR 101 <rpt-file-err> NEW 4 3 1 !  \
	\

<report-name-option>   ::=
	<report-name>  ! UP 1 !  \
	<NULL>  ! UP 1 !  \
	\

<report-name>   ! NV$RPTNAM !   ::=
	<name>  ! UP 1 !  \
	\

<append-option>  ::=
	APPEND  ! UP 1 !  \
	<NULL>  ! UP 1 !  \
	\

<file>   ! NV$FILE !   ::=
	<quoted-string>  ! UP 1 !  \
	<field-name>  ! UP 1 !  \
	\

<report-param-option>   ::=
	<report-param-list>  ! NEW 1 !  \
	<NULL>  ! NEW !  \
	\

<report-param-list>   ::=
	<report-param>  ! UP 1 NOBACKUP !
	  <report-param-list>  ! APPEND 1 !  \
	<report-param>  ! UP 1 NOBACKUP !  \
	\

<report-param>   ::=
	<report-arith-item> <arith-exp>  ! ERROR 101 <report-ae>
	  NEW 1 2 ALT NV$RAI !  \
	<report-char-item> <char-literal>  ! ERROR 101 <report-NULL>
	  NEW 1 2 ALT NV$RCI !  \
	\

<report-arith-item>   ::=
	LINE.LENGTH  ! UP 1 !  \
	BOTTOM.MARGIN  ! UP 1 !  \
	FOOTING.SIZE  ! UP 1 !  \
	LINE.NUM  ! UP 1 !  \
	PAGE.NUM  ! UP 1 !  \
	PAGE.SIZE  ! UP 1 !  \
	TOP.MARGIN  ! UP 1 !  \
	HEADING.SIZE  ! UP 1  ! \
	\

<report-char-item>   ::=
	PAGE.DIVIDER  ! UP 1 !  \
	PAGE.TOP  ! UP 1 !  \
	\

<report-cond-def-option>   ::=
	<report-cond-def-list>  ! NEW 1 !  \
	<NULL>  ! NEW !  \
	\

<report-cond-def>   ::=
	<report-cond>
	  <stmt>  ! ERROR 101 <stmt-err> !
	  <CREATE>  ! NDRPC NEW 3 2 1 ALT NV$RPT !  \
	<group-cond>
	  <stmt>  ! ERROR 101 <stmt-err> !
	  <CREATE>  ! NDRPC NEW 3 2 1 ALT NV$GRP !  \
	\

<report-cond-def-list>   ::=
	<report-cond-def>  ! UP 1 NOBACKUP !
	  <report-cond-def-list>  ! APPEND 1 !  \
	<report-cond-def>  ! UP 1 NOBACKUP !  \
	\

<report-cond>   ! NV$RCOND !   ::=
	REPORT.HEADING  ! UP 1 ! \
	REPORT.FOOTING  ! UP 1 !  \
	PAGE.HEADING  ! UP 1 !  \
	PAGE.FOOTING  ! UP 1 !  \
	\

<group-cond>   ::=
	GROUP.HEADING
	  <cond-name-option> ON  ! ERROR 100 <end-stmt> !
	  <field-name>  ! ERROR 101 <end-fld> UP 3 1 4 !  \
	GROUP.FOOTING  
  	  <cond-name-option> ON  ! ERROR 100 <end-stmt> !
	  <field-name>  ! ERROR 101 <end-fld> UP 3 1 4 !  \
	\

<cond-name-option>   ::=
	<cond-name>  ! UP 1 !  \
	<NULL>  ! UP 1 !  \
	\

<cond-name>   ! NV$CNDNAM !   ::=
	<name>  ! UP 1 !  \
	\

<PRINT-stmt>   ::=
	TO <report-name>  ! ERROR 101 <end-stmt>  !
	  <output-elist>  ! UP 2 3 1 !  \
	<output-elist>  ! UP 1 !  \
	\

&! BACKUPOK because can be in <arith-exp>
<report-data>   ::=
	<report-item> OF <report-name>  ! ERROR 101 <end-ae> UP 1 3 
	  BACKUPOK !  \
	<report-item> <NULL>  ! UP 2 !  \    &! COMMONALITY
	\

<report-item>   ::=
	<report-arith-item>  ! UP 1 !  \
	<report-char-item>  ! UP 1 !  \
	\

<FORCE-stmt>   ::=
	FOR <report-name>  ! ERROR 101 <end-stmt> !
	  <report-cond>  ! ERROR 101 <end-cond> UP 2 1 !  \
	<cond-name>   ! UP 1 !  \
	<report-cond>  ! ERROR 101 <end-cond> UP 1 !  \
	\

<labelled-stmt>   ::=
	<label>  ! UP 1 CALL $BWTRE !
	  <stmt>  ! ERROR 101 <stmt-err> APPEND 1 !  \
	\

<IF-stmt>   ::=
	IF <cond-exp>  ! ERROR 101 <IF-cond-err> !
	  <CREATE>  ! BGIF UP 1 2 3 CALL $BWTRE !
	  <then-clause>  ! APPEND 1 !  \
	\

<then-clause>   ::=
	THEN  ! ERROR 100 <THEN-err> !
	  <stmt>  ! ERROR 101 <stmt-err> !
	  ELSE  ! UP 2 3 CALL $BWTHN !
	  <stmt>  ! ERROR 101 <stmt-err> !
	  <CREATE>  ! ELSE APPEND 2 1 !  \
	THEN  ! ERROR 100 <THEN-err> !
	  <stmt>  ! ERROR 101 <stmt-err> !
	  <NULL>  ! UP 2 3 !  \   &! COMMONALITY
	\

<WHILE-DO-stmt>   ::=
	<cond-exp>  ! ERROR 101 <end-cond-exp> !
	  DO  ! ERROR 100 <DO-err> !
	  <CREATE>  ! WLDO !
	  <CREATE>  ! BGWLDO UP 1 4 2 CALL $BWTRE !
	  <stmt>  ! ERROR 101 <stmt-err> APPEND 1 !  \
	\

<DO-WHILE-stmt>   ::=
	<CREATE>  ! BGDOWL UP 1 CALL $BWTRE !
	  <stmt>  ! ERROR 101 <stmt-err> !
	  WHILE  ! ERROR 100 <WHILE-err> !
	  <cond-exp>  ! ERROR 101 <end-cond-exp> !
	  <CREATE>  ! DOWL APPEND 4 2 1 !  \
	\

<FINISH-stmt>   ::=
	<label-name>  ! ERROR 101 <end-fld> UP 1 !  \
	\

<REPEAT-stmt>   ::=
	<label-name>  ! ERROR 101 <end-fld> UP 1 !  \
	\

<label-name>   ! NV$LABNAM !   ::=
	<name>  ! UP 1 !  \
	\

<ABORT-stmt>   ::=
	<output-list>  ! UP 1 !  \
	<NULL>  \
	\

<ACCEPT-stmt>   ::=
	<field-name-elist>  ! UP 1 !  \
	\

<TYPE-stmt>   ::=
	<output-elist>  ! UP 1 !  \
	\

<MOVE-stmt>   ::=
	<valued-exp>  ! ERROR 101 <move-ve-err> !
	  TO  ! ERROR 100 <TO-err> !
	  <variable-t-elist>
	  <round-spec>  ! UP 2 4 1 !  \
	\

<LET-stmt>   ::=
	<variable-t-elist>
	  <round-spec> =  ! ERROR 100 <=-err> !
	  <valued-exp>  ! ERROR 101 <move-ve-err> UP 4 1 3 !  \
	\

<ADD-stmt>   ::=
	<arith-exp>  ! ERROR 101 <add-ae-err> !
	  TO  ! ERROR 100 <TO-err> !
	  <variable-t-elist>
	  <round-spec>  ! UP 2 4 1 !  \
	\

<SUBTRACT-stmt>   ::=
	<arith-exp>  ! ERROR 101 <add-ae-err> !
	  FROM  ! ERROR 100 <FROM-err> !
	  <variable-t-elist>
	  <round-spec>  ! UP 2 4 1 !  \
	\

<round-spec>   ::=
	ROUNDED  ! UP 1 !  \     &! ROUND  ! UP 1 !  \
	TRUNCATED  ! UP 1 !  \     &! TRUNC  ! UP 1 !  \
	<NULL> <CREATE>  ! ROUNDED UP 1 !  \
	\

<variable-t-elist>   ::=
	<variable-t-elist-1> <CREATE>  ! VLST !
	  <CREATE>  ! BGVLST UP 1 3 2 !  \
	\

<variable-t-elist-1>   ::=
	<variable-t>  ! ERROR 101 <scan-vart-list> UP 1 !  
	 , <variable-t-elist-1>  ! APPEND 1 !  \
	<variable-t>  ! ERROR 101 <scan-vart-list> UP 1 !  \
	\

<variable-t>   ! NV$VART !   ::=
	<qualified-name>  ! UP 1 !  \
	\


&! BEGIN <cond-exp>     (NOTE "PROCEDURE ONLY" COMMENTS)

<cond-exp>   ! NV$CONDE !   ::=
	<bool-exp> <NOTANY>  ! <and-or> UP 2 !  \
	<ext-rel>  ! UP 1 !  \
	\

<and-or>   ::=   AND \ OR \ \

<bool-exp>  ::=
	<boolean-factor>  ! UP 1 !  \
	<bool-exp> OR <boolean-factor>  ! APPEND 1 2 !  \
	\

<boolean-factor>   ::=
	<boolean-element>  ! UP 1 !  \
	<boolean-factor> AND <boolean-element>  ! APPEND 1 2 !  \
	\

<boolean-element>   ::=
        <qualified-name> CHANGING  ! UP 2 1 !  \
	<qualified-name> NOT CHANGING  ! ERROR 100 <ae-CHANGING> UP 3 1 2
	  BACKUPOK !  \
	<qualified-name> IS NOT <picture-ref>  ! ERROR 101 <ae-picture> UP 4 1 3 2 
	  BACKUPOK !  \
	<qualified-name> IS <picture-ref>  ! ERROR 101 <ae-picture> UP 3 1 2 
	  BACKUPOK !  \
	<qualified-name> ILLEGAL  ! UP 2 1 !  \
	( <cond-exp> )  ! ERROR 100 <ae-rparen> UP 2 BACKUPOK !  \
	NOT (  ! ERROR 100 <ae-lparen> BACKUPOK !
	  <cond-exp>  ! ERROR 101 <ae-cond> BACKUPOK !
	  )  ! ERROR 100 <ae-rparen> UP 2 4 BACKUPOK !  \
	<rel-exp>  ! UP 1 !  \
	\

<picture-ref>   ! NV$PIC !   ::=
	<field-name>  ! UP 1 !  \
	<picture>  ! UP 1 !  \
	\

<rel-exp>   ::=
	<valued-exp> <rel-op> <valued-exp>  ! ERROR 101 <ae-0-err>
	  UP 3 1 2 BACKUPOK !  \
	\

<ext-rel>   ::=
	<ext-rel-1>  ! UP 1 !  \
	<ext-rel-2>  ! UP 1 !  \
	<ext-rel-3>  ! UP 1 !  \
	<ext-rel-4>  ! UP 1 !  \
	\

<ext-rel-1>   ::=
	<valued-exp> <rel-op> <valued-exp>  ! ERROR 101 <ae-0-err> BACKUPOK !
	  AND <rel-op> <valued-exp>  ! ERROR 101 <ae-0-err> BACKUPOK !
	  <CREATE>  ! EXT CALL $BEXTP BACKUPOK !
	  <NULL>  ! UP 8 2 6 7 3 4 5 !  \
	<ext-rel-1> AND <rel-op> <valued-exp>  ! ERROR 101 <ae-0-err>
	  APPEND 1 2 3 CALL $BEXCT BACKUPOK !  \
	\

<ext-rel-2>   ::=
	<valued-exp> <rel-op>  ! SUBID TV$8EXT !
	  <valued-exp>  ! ERROR 101 <ae-0-err> BACKUPOK !
	  AND <valued-exp>  ! ERROR 101 <ae-0-err> BACKUPOK !
	  <CREATE>  ! EXT CALL $BEXTP BACKUPOK !
	  <NULL>  ! UP 7 2 5 6 3 4 !  \
	<ext-rel-2> AND <valued-exp>  ! ERROR 101 <ae-0-err>
	  APPEND 1 2 CALL $BEXCT BACKUPOK !  \
	\

<ext-rel-3>   ::=
	<valued-exp> <rel-op> <valued-exp>  ! ERROR 101 <ae-0-err> BACKUPOK !
	  OR <rel-op> <valued-exp>  ! ERROR 101 <ae-0-err> BACKUPOK !
	  <CREATE>  ! EXT CALL $BEXTP BACKUPOK !
	  <NULL>  ! UP 8 2 6 7 3 4 5 !  \
	<ext-rel-3> OR <rel-op> <valued-exp>  ! ERROR 101 <ae-0-err>
	  APPEND 1 2 3 CALL $BEXCT BACKUPOK !  \
	\

<ext-rel-4>   ::=
	<valued-exp> <rel-op>  ! SUBID TV$8EXT !
	  <valued-exp>  ! ERROR 101 <ae-0-err> BACKUPOK !
	  OR <valued-exp>  ! ERROR 101 <ae-0-err> BACKUPOK !
	  <CREATE>  ! EXT CALL $BEXTP BACKUPOK !
	  <NULL>  ! UP 7 2 5 6 3 4 !  \
	<ext-rel-4> OR <valued-exp>  ! ERROR 101 <ae-0-err>
	  APPEND 1 2 CALL $BEXCT BACKUPOK !  \
	\

<rel-op>  ::=
	LT  ! UP 1 !  \    &! <   ! UP 1 !  \
	LE  ! UP 1 !  \    &! <=  ! UP 1 !  \
	NE  ! UP 1 !  \    &! #   ! UP 1 !  \
	GE  ! UP 1 !  \    &! >=  ! UP 1 !  \
	GT  ! UP 1 !  \    &! >   ! UP 1 !  \
	EQ <CREATE>  ! = UP 1 !  \
	=   ! UP 1 !  \
	\

<valued-exp>   ! NV$VE !   ::=
	DEFAULT  ! UP 1 !  \
	<arith-exp>  ! UP 1 !  \
	\

<arith-exp>   ! NV$AE !   ::=
	<arith-exp-1>
	  <CREATE>  ! BGAE !
	  <CREATE>  ! NDAE UP 2 3 1 !  \
	\

<arith-exp-1>   ! NV$AE !   ::=
	<term>   ! UP 1 !   \
	<arith-exp-1> + <term>  ! ERROR 101 <term-err> APPEND 1 2 
	  BACKUPOK !  \
	<arith-exp-1> - <term>  ! ERROR 101 <term-err> APPEND 1 2 
	  BACKUPOK !  \
	\

<term>   ! NV$AE !   ::=
	<primary>   ! UP 1 !   \
	<term> * <primary>  ! ERROR 101 <prim-err> APPEND 1 2 BACKUPOK !   \
	<term> / <primary>  ! ERROR 101 <prim-err> APPEND 1 2 BACKUPOK !   \
	\

<primary>   ! NV$AE !   ::=
	<secondary>  ! UP 1 !   \
	+ <secondary>  ! ERROR 101 <prim-err> UP 1 BACKUPOK !   \
	- <secondary>  ! ERROR 101 <prim-err> BACKUPOK !
	 <CREATE>  ! UMINUS UP 2 1 !   \
	\

<secondary>   ! NV$AE !   ::=
	<tertiary> <format-picture>  ! UP 2 1 !  \
	<tertiary> <NULL>  ! UP 2 !  \    &! COMMONALITY
	\

<tertiary>   ::=
	<data>  ! UP 1 !  \
	( <arith-exp-1> )  ! ERROR 100 <ae-rparen> UP 2 BACKUPOK !  \
	\

<format-picture>   ::=
	AS <field-name>  ! UP 1 2 !  \
	AS <picture>  ! ERROR 101 <prim-pic> UP 1 2 BACKUPOK !  \
	\

<data>   ::=
	<ident-ref>   ! UP 1 !   \
	<report-data>   ! UP 1 !   \    &! PROCEDURE ONLY
	<function>   ! UP 1 !   \
        <number>  ! UP 1 !  \
	<char-literal>  ! UP 1 !  \
	<date-literal>  ! UP 1 !  \
	\

<ident-ref>   ! NV$FLDNAM !   ::=
	<field-name> ( <via-qualified-relation>  ! ERROR 101 <ae-qrel>
	 BACKUPOK !
	 )  ! ERROR 100 <ae-rparen> BACKUPOK !
	 <CREATE>   ! QUAL SUBID TV$8TWO UP 5 3 1 !   \
	<qualified-name>   ! UP 1 !   \
	\

<qualified-name>   ! NV$FLDNAM !   ::=
	<field-name> OF <name>  ! ERROR 101 <ae-rel> UP 3 1 2 
	  BACKUPOK !  \
	<field-name> <NULL>  ! UP 2 !  \   &! COMMONALITY
	\

<via-qualified-relation>   ! NV$RELNAM !   ::=
	<relation-name> <via-clause>  ! ERROR 101 <ae-viacl> UP 2 1 !  \
	\

<qualified-relation>   ! NV$RELNAM !   ::=
	<relation-name> <qualification>  ! UP 2 1 !  \
	\

<qualification>   ::=
	<via-clause> <condition-option>  ! UP 2 1 !  \
	<condition> <CREATE>  ! VIA SUBID TV$8ONE UP 1 2 !  \
	<NULL> <CREATE>  ! VIA SUBID TV$8ONE UP 1 !  \
	\

<condition-option>   ::=
	, <condition>  ! ERROR 101 <end-ae> UP 1 BACKUPOK !  \
	<NULL>  \
	\

<via-clause>   ! NV$VIACL !   ::=
	VIA <via-type>  ! ERROR 101 <ae-KEY> UP 1 2 CALL $BMVCT 
	  BACKUPOK !  \
	\

<via-type>   ! NV$VIATYP !   ::=
	KEY  ! UP 1 !  \
&!	KEYS   ! UP 1 !  \
	<via-field-list>  ! UP 1 !  \
	\

<condition>   ! NV$COND !  ::=
	WHERE <cond-exp>  ! ERROR 101 <ae-cond> BACKUPOK !
	 <CREATE>  ! BGWHR UP 1 2 3 !  \
	\

<via-field-list>   ::=
	<key-value-1>  ! UP 1 !  \
	( <field-name-listc> ) (  ! UP 3 CALL $BDPCT BACKUPOK !
	 <qualified-relation>  ! ERROR 101 <ae-qrel> BACKUPOK !
	 )  ! ERROR 100 <ae-rparen> BACKUPOK !
	 <CREATE>  ! QUAL APPEND 3 1 CALL $BMVCT BACKUPOK !  \
	( <key-value-list>  ! CALL $BDPCT BACKUPOK !
	  )  ! ERROR 100 <ae-rparen> UP 2 BACKUPOK !  \
	\

<field-name-listc>   ::=
	<field-name-new>  ! UP 1 !  \
	<field-name-listc>
	  , <field-name-new>  ! APPEND 1 !  \
	\

<field-name-new>   ::=
	<field-name>  ! NEW 1 !  \
	\

<key-value-list>   ::=
	<key-value>  ! UP 1 !  \
	<key-value-list>
	  , <key-value>  ! APPEND 1 !  \
	\

<key-value>   ::=
	<field-name> = <valued-exp>  ! ERROR 101 <ae-ve> BACKUPOK !
	 <CREATE>  ! = SUBID TV$LET NEW 4 2 1 !  \
	<ident-ref>  ! NEW 1 !  \
	\

<key-value-1>   ::=
	<field-name> = <valued-exp>  ! ERROR 101 <ae-ve> BACKUPOK !
	  <CREATE>  ! = SUBID TV$LET UP 4 2 1 !  \
	<ident-ref>  ! UP 1 !  \
	\

<literal>   ! NV$LIT !   ::=
	<char-literal>  ! UP 1 !  \
	<date-literal>  ! UP 1 !  \
	<number>  ! UP 1 !  \
        + <number>  ! UP 1 !  \
        - <number>  ! UP 1 CALL $BNEG !  \
	\

<char-literal>   ! NV$CLIT !   ::=
	<quoted-string>  ! UP 1 !  \
	<@integer>  ! UP 1 !  \
	@CR  ! UP 1 !  \
	\

<date-literal>   ::=
	DATE <quoted-string>  ! UP 1 CALL $BDATP BACKUPOK !  \
	\

<number>   ::=
	<integer>  ! UP 1 !  \
	<dec-number>  ! UP 1 !  \
	\

<function>   ::=
	SUM (  ! ERROR 100 <ae-nparen> BACKUPOK !
	  <qualified-name>  ! ERROR 101 <ae-fld> BACKUPOK !
	  )  ! ERROR 100 <ae-rparen> UP 2 4 BACKUPOK !  \
	AVG (  ! ERROR 100 <ae-nparen> BACKUPOK !
	  <qualified-name>  ! ERROR 101 <ae-fld> BACKUPOK !
	  )  ! ERROR 100 <ae-rparen> UP 2 4 BACKUPOK !  \
	MIN (  ! ERROR 100 <ae-nparen> BACKUPOK !
	  <qualified-name>  ! ERROR 101 <ae-fld> BACKUPOK !
	  )  ! ERROR 100 <ae-rparen> UP 2 4 BACKUPOK !  \
	MAX (  ! ERROR 100 <ae-nparen> BACKUPOK !
	  <qualified-name>  ! ERROR 101 <ae-fld> BACKUPOK !
	  )  ! ERROR 100 <ae-rparen> UP 2 4 BACKUPOK !  \
	COUNT (
	  <relation-name>  ! ERROR 101 <ae-rel> BACKUPOK !
	  )  ! ERROR 100 <ae-rparen> BACKUPOK !
	  <CREATE>  ! COUNT UP 3 1 !  \   &! COMMONALITY
	COUNT <CREATE>  ! COUNTN UP 1 !  \
	TODAY  ! UP 1 !  \
	TIME  ! UP 1 !  \
	\

&! END <cond-exp>


<TRIM-function>   ::=
	TRIM (  ! ERROR 100 <list-nparen> !
	  <arith-exp>  ! ERROR 101 <list-fld-err> !
	  )  ! ERROR 100 <list-rparen> UP 2 4 !  \
	\


&!  ERROR PRODUCTIONS

<PROC-err>   ::=
	PROCEDURE  ! UP 1 !  \
	(  ! RESCAN !  <CREATE>  ! PROCEDURE UP 1 !  \
	<new-stmt>  <CREATE>  ! PROCEDURE UP 1 !  \
	<ANY>  ! NOBACKUP !  <PROC-err>  ! UP 1 !  \
	\

<proc-name-err>   ::=
	<name>  ! UP 1 !  \
	(  ! RESCAN !  <new-name>  ! UP 1 !  \
	<new-stmt> <new-name>  ! UP 1 !  \
	<ANY>  ! NOBACKUP !  <proc-name-err>  ! UP 1 !  \
	\

<new-name>   ::=
	<CREATE>  ! <name> SUBID TV$7NEW UP 1 CALL $BETOK !  \
	\

<rparen-err>   ::=
	)  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <rparen-err>  \
	\

<stmt-err>   ::=
	<end-stmt> <CREATE>  ! NOTHING UP 1 CALL $BSTMT !  \
	\

<stmts-to-eof>   ::=
	<eof>  ! UP 1 !  \
	<new-stmt> <stmt-list> <eof>  
	  ! ERROR 101 <stmts-to-eof> UP 2 1 !  \
	<new-stmt> <decl-list> <stmt-list-option> <eof>
	  ! ERROR 101 <stmts-to-eof> UP 2 1 !  \
	<ANY>  ! NOBACKUP !  <stmts-to-eof>  ! UP 1 !  \
	\

&! ISOLATE <eof> SO DON'T LOOK FOR STMT-LIST AFTER IT
<stmts-to-END>   ::=
	END  ! UP 1 !  \
        <eof> <CREATE>  ! END UP 1 !  \
	<new-stmt> <stmt-list> END  
	  ! ERROR 100 <stmts-to-END> UP 2 1 !  \
	<new-stmt> <decl-list> <stmt-list-option> END
	  ! ERROR 100 <stmts-to-END> UP 2 1 !  \
	<ANY>  ! NOBACKUP !  <stmts-to-END>  ! UP 1 !  \
	\

<decl-list-option>   ::=
	<decl-list>  \
	<NULL>  \
	\


&! BEGIN <schema-decl> ERRORS   (NOTE <formal-param> COMMENTS)

<fld-dec-err>   ::=
	<scan-fld-dec> <new-name>  ! UP 1 !  \
	\

<attr-err>   ::=
	<scan-fld-dec> <default-fld> <NULL-constraints> <NULL>
	  ! NEW 3 2 1 ALT NV$SAME !  \
	\

<scan-fld-dec>   ::=
	SAME  ! RESCAN !  \
	AS  ! RESCAN !  \
	,  ! RESCAN !  \    &! <formal-param>
	)  ! RESCAN !  \    &! <formal-param>
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-fld-dec>  \
	\

<scan-attr>   ::=
	VERIFY  ! RESCAN  !  \    &! <field-def>
	DEFAULT  ! RESCAN !  \
	,  ! RESCAN !  \
	)  ! RESCAN !  \    &! <formal-param>
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-attr>  \
	\

<attr-fld>   ::=
	<scan-attr> <default-fld>  ! UP 1 !  \
	\

<attr-pic>   ::=
	<scan-attr> <default-picture>  ! UP 1 !  \
	\

<default-picture>   ::=
	<CREATE>  ! <quoted-string> SUBID TV$7PIC CALL $BETOK 
	  BACKUPOK !  <CREATE>  ! PIC UP 2 1 !  \
	\

<attr-NULL>   ::=
	<scan-attr> <NULL>  ! UP 1 !  \
	\

<default-fld>   ::=
	<CREATE>  ! <name> SUBID TV$7NAME UP 1 CALL $BETOK 
	  BACKUPOK !  \
	\

<rel-dec-err>   ::=
	<scan-rel-dec> <new-name>  ! UP 1 !  \
	\

<rel-def-err>   ::=
	<scan-rel-dec> <default-rel>  ! NEW 1 ALT NV$SAME !  \
	\

<scan-rel-dec>   ::=
	SAME  ! RESCAN !  \
	IS  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-rel-dec>  \
	\

<end-stmt>   ::=
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <end-stmt>  \
	\

<end-rel>   ::=
	<end-stmt> <default-rel>  ! UP 1 !  \
	\

<KEY-err>   ::=
	KEY  \
	DATA  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <KEY-err>  \
	\

<scan-list>   ::=
	,  ! RESCAN !  \
	DATA  ! RESCAN !  \       &! <key-list>
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-list>  \
	\

<default-rel>   ::=
	<CREATE>  ! <name> SUBID TV$7REL UP 1 CALL $BETOK 
	  BACKUPOK !  \
	\

&! END <schema-decl> ERRORS


<edit-rel-err>   ::=
	<scan-edit-stmt> <default-rel>  ! UP 1 !  \
	\

