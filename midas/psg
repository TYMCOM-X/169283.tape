	TO  \
	<report-param-rescan>  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <rpt-TO-err>  \
	\

<scan-report>   ::=
	<report-param-rescan>  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-report>  \
	\

<rpt-file-err>   ::=
	<scan-report> <default-file>  ! UP 1 !  \
	\

<report-ae>   ::=
	<scan-report> <ae-1>  ! UP 1 !  \
	\

<report-NULL>   ::=
	<scan-report> <NULL>  ! UP 1 !  \
	\

<report-param-rescan>   ::=
	LINE.LENGTH  ! RESCAN !  \      
	BOTTOM.MARGIN  ! RESCAN !  \    
	FOOTING.SIZE  ! RESCAN !  \     
	LINE.NUM  ! RESCAN !  \         	
	PAGE.NUM  ! RESCAN !  \         	
	PAGE.SIZE  ! RESCAN !  \        
	TOP.MARGIN  ! RESCAN !  \       
	HEADING.SIZE  ! RESCAN !  \     
	PAGE.TOP  ! RESCAN !  \         	
	PAGE.DIVIDER  ! RESCAN !  \     
	\

<scan-vart-list>   ::=
	,  ! RESCAN !  \
	ROUNDED  ! RESCAN !  \
	TRUNCATED  ! RESCAN !  \
	=  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-vart-list>  \
	\

<end-cond>   ::=
	<end-stmt> <CREATE>  ! REPORT.HEADING UP 1 !  \
	\

<IF-cond-err>   ::=
	<scan-THEN> <default-cond-exp>  ! UP 1 !  \
	\

<end-cond-exp>   ::=
	<end-stmt> <default-cond-exp>  ! UP 1 !  \
	\

<THEN-err>   ::=
	THEN  ! UP 1 !  \
	<new-stmt> <CREATE>  ! THEN UP 1 !  \
	<ANY>  ! NOBACKUP !  <THEN-err>  ! UP 1 !  \
	\

<DO-err>   ::=
	DO  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <DO-err>  \
	\

<WHILE-err>   ::=
	WHILE  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <WHILE-err>  \
	\

<scan-move>   ::=
	TO  ! RESCAN !  \
	FROM  ! RESCAN !  \
	ROUNDED  ! RESCAN !  \
	TRUNCATED  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <scan-move>  \
	\

<move-ve-err>   ::=
	<scan-move> <CREATE>  ! DEFAULT UP 1 CALL $BETOK !  \
	\

<add-ae-err>   ::=
	<scan-move> <ae-0>  ! UP 1 !  \
	\

<TO-err>   ::=
	TO  \
	ROUNDED  ! RESCAN !  \
	TRUNCATED  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <TO-err>  \
	\

<=-err>   ::=
	=  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <=-err>  \
	\

<FROM-err>   ::=
	FROM  \
	ROUNDED  ! RESCAN !  \
	TRUNCATED  ! RESCAN !  \
	<new-stmt>  \
	<ANY>  ! NOBACKUP !  <FROM-err>  \
	\


&! BEGIN <cond-exp> ERRORS

<scan-ae>   ::=
	LT  ! RESCAN !  \
	LE  ! RESCAN !  \
	NE  ! RESCAN !  \
	GE  ! RESCAN !  \
	GT  ! RESCAN !  \
	EQ  ! RESCAN !  \
	=  ! RESCAN !  \
	,  ! RESCAN !  \
	)  ! RESCAN !  \
	AS  ! RESCAN !  \
	AND  ! RESCAN !  \
	OR  ! RESCAN !  \
	DEFAULT  ! RESCAN !  \
	TO  ! RESCAN !  \       &! PROCEDURE ONLY
	FROM  ! RESCAN !  \     &! PROCEDURE ONLY
	THEN  ! RESCAN !  \     &! PROCEDURE ONLY
	ALL  ! RESCAN !  \      &! PROCEDURE ONLY
	SORTED  ! RESCAN !  \   &! PROCEDURE ONLY
	INPUT  ! RESCAN !  \    &! PROCEDURE ONLY
	<report-param-rescan>  \      &! PROCEDURE ONLY
	<new-stmt>  \
	\

