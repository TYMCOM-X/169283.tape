MODULE GAMO(DOLLAR, ENTRIES=($COUTG), FSAVE,TIMING,
                        TIMER=EXTERNAL(SIX12) )=
BEGIN

REQUIRE KDEFS.BLI[7,107355];
REQUIRE CDEFS.BLI[7,107355];
REQUIRE FDEFS.BLI[7,107355];
REQUIRE GDEFS.BLI[7,107355];

UNDECLARE $COUTG;


GLOBAL ROUTINE $COUTG(BUF)=
  BEGIN
  MAP G$ BUF;
  LOCAL TKNSIZ;
  LOCAL KFCB$ FCB;
  IF (TKNSIZ _ .BUF[G$LENGTH]) GTR GV$MAXGREC  OR
      .TKNSIZ LEQ 0
    THEN %INSERT CODE HERE TO EXPAND ON ERROR USING UOUT%
         $FATAL(#420);
  FCB_.CFLCB$[CV$GAMMAOUT];
  FCB[K$DCNT]_.TKNSIZ;
  FCB[K$DADR]_.BUF;
  $KWRITE(FCB);
  CACTI$[CV$GAMMAOUT]_.CACTI$[CV$GAMMAOUT]+1;
  RETURN .TKNSIZ
  END;

%THIS ROUTINE WILL EVENTUALLY BE EXPANDED TO ALLOW FOR
 TRANSFER OF TOKENS TO CORE AS AN ALTERNATIVE TO DISK WHEN
 "FILE" SIZE PERMITS.
%

END
ELUDOM
