MODULE RSLCT(DOLLAR,ENTRIES=($RSLCT),FSAVE,TIMING,TIMER=EXTERNAL(SIX12))=

BEGIN

REQUIRE RCDEFS.BLI[7,107355];
REQUIRE RDEFS.BLI[7,107355];
REQUIRE KDEFS.BLI[7,107355];
REQUIRE SBDEFS.BLI[7,107355];
REQUIRE UDEFS.BLI[7,107355];
REQUIRE FDEFS.BLI[7,107355];
UNDECLARE $RSLCT;

GLOBAL ROUTINE $RSLCT(RCB)=

   BEGIN
   MAP RCB$ RCB;
   LOCAL KFCB$ FCB;
   LOCAL BA,RET;

   FCB_.RCB[RC$FCB];
   FCB[K$SOBP]_.RCB[RC$SOBP];
   $KPRON(.FCB,BA_FCB[K$SBP]_$KVIRD(.FCB,SBV$SBPN,SBV$SBSZ),SBV$SBPN,SBV$SBSZ);
   IF $KRDKY(.FCB) 
      THEN RET_ RCV$FALSE
      ELSE RET_ RCV$TRUE;
   $KPROF(.FCB,.BA,SBV$SBPN,SBV$SBSZ);
   RETURN .RET
   END;
END ELUDOM
