MODULE KDALL(DOLLAR,ENTRIES=($KDALL),FSAVE,TIMING,TIMER=EXTERNAL(SIX12))=

BEGIN

REQUIRE KDEFS.BLI[7,107355];
REQUIRE SBDEFS.BLI[7,107355];
UNDECLARE $KDALL;

GLOBAL ROUTINE $KDALL(FCB)=

   BEGIN
   MAP KFCB$ FCB;
   LOCAL KSB$ SB;

   SB_.FCB[K$SBP];
   SB[SB$NOL]_SB[SB$NINS]_SB[SB$TOP]_SB[SB$1ST]_0;
   INCR I FROM 1 TO .SB[SB$TOTAL]-1
      DO $KRELP(.FCB,.I);
   $KACWR(.FCB,.SB,SBV$SBPN);
   $KNFLS(.FCB);
   END;
END ELUDOM
