  EXTERNAL $GGPFT, $GERPC, $GBFRC, $GFRCE, $GFIN ,$GRPET, $GVIA ,$GQUAL;
  EXTERNAL $GBWHR, $GWHR , $GOF  , $GAS  , $GBLNK,$GFORM, $GSTRP,$GRITM;
