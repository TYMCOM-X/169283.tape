  EXTERNAL $GSUM , $GAVG , $GMIN , $GMAX , $GDATE,$GDAT3, $GTRIM,$GCTIM;
  EXTERNAL $GTYMD, $GVRF , $GNTHG;
  EXTERNAL $GACC , $GEDOW, $GBSRT, $GZDMP;
  EXTERNAL $GEOF, $GTIME;
  EXTERNAL $GOHOH, $GDFLT, $GTDAY;
  EXTERNAL $GNNAME, $GBWL;
  EXTERNAL $GUSNG, $GUPD, $GBUPD, $GFOR;
  EXTERNAL $GBRPT, $GWRPT, $GERPT;
  EXTERNAL $GEQ, $GCNTN, $GCTRL, $GNOOP, $GTO;

% THE FOLLOWING EXTERNALS ARE DEFINED IN MODULE GBUFM.BLI  %
  EXTERNAL $GIBUF, $GNBUF, $GRBUF, $GWBUF, $GMBUF, $GDBUF, $GFBUF, $GCBUF;

