        DOUBLE PRECISION NAME
      WRITE(5,10)
   10 FORMAT(' FILE NAME ? ',$)
      READ(5,20) NAME
  20  FORMAT(A10)
      CALL BFILE(20,NAME,0,'INPUT')
      CALL BFILE(21,'HALF1.XX',0,'OUTPUT')
      CALL BFILE(22,'HALF2.XX',0,'OUTPUT')
      REAL A(26)
*
*
 25     FORMAT(26A5)
      DO 100 I=1,1000
      LINES=LINES+1
      READ(20,25,END=86) A
  26    FORMAT(' REPORT HAS ',I5,//)
      WRITE(21,25) (A(J),J=1,14)
      WRITE(22,25) (A(J),J=15,26)
 100  CONTINUE
*
*
  86  WRITE(5,26) LINES
      STOP
      END
