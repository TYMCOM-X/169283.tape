        LOGICAL FUNCTION MODFIND(NOARGS)
C
        COMMON/MODELS/NUMMOD,MODNAM(25),KIND(25)
        COMMON/PRISET/ITEM,NAMELE,ISTART,ISTOP,IPALL
        LOGICAL IPALL
C
        MODFIND=.FALSE.
        DO 10 ISTART=1,NUMMOD
        IF(MODNAM(ISTART).EQ.NAMELE)GO TO 20
10      CONTINUE
        RETURN
20      ISTOP=ISTART
        MODFIN=.TRUE.
        RETURN
        END
