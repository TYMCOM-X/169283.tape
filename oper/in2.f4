 3012 WRITE (W,9609)
 9609 FORMAT(T15,'TRAILER REAR TANDEM BRAKES')
 3000 WRITE (W,9610) NUM(I),(XX(IJ),YY(IJ),IJ=KKKF,KKF)
 9610 FORMAT(T15,'NO. OF POINTS:',I3/' ',25(T6,F12.4,2X,F12.4/))
      DO 4014 KI=KKKF,KKF
 4014 YY(KI)=YY(KI)/12.
 4013 CONTINUE
 3604 CONTINUE
