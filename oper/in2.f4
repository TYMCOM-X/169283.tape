      SUBROUTINE INPUT(Y,DERY,PRMT,NDIM,AUX)
C
C DECLARATION STATEMENTS
C
      INTEGER R,W
      REAL J,JS,K,KT,J1,KF,M,MS,MU,M1,MUZERO,N,NS,L,MAXAX,MAPBX,MAPBY
      REAL M1,M2,MS
      REAL*8 DCS,DJS,DKT,DWS
      COMMON /PAR/ AA1,AA2,AA3,AA4,AA5,AA6,AA7,AA8,AA9,AA10,AA11,AA12,
     1  AA13,AA14,AA15,AA16,A1,A2,A3,A4,ALPHA,BB,C,CF,CS,D,D1,DELTA,FA,
     2  J,J1,JS,K,KF,KT,MUZERO,PERCNT,PW,PJ,PX,PZ,W1,W2,WS ,M1,M2,MS,PM
      DIMENSION ALPHA(3),C(6),CF(3),CS(5),DELTA(3),FA(5),JS(5),K(3),
     1  KT(5),MUZERO(5),WS(5)
      COMMON /KEY/ KEY,KAXLE,KROAD
      COMMON /MAPB/ KMAPB,NOMAPB,MAPBX,MAPBY
      COMMON /PAGE/ NP,HEAD
      COMMON /SAFB/ KSAFB,ONTIME,TMAX,RISET
      COMMON /BLOCK1/ R,W
      COMMON /BLOCK3/ ICOUNT,NOGEAR,GEARV,GRAT,NUMEB,EBX,EBY,EBPER,
     1  EMINUS
      COMMON /BLOCK6/ TQ,XX,YY,NUM
      COMMON /BLOCK8/ KPROP,MAXAX,EMPTY
      DIMENSION MS(5)
      DIMENSION Y(20),DERY(20),PRMT(5),AUX(16,20)
      DIMENSION KEY(2),PERCNT(2)
      DIMENSION HEAD(20)
      DIMENSION MAPBX(5),MAPBY(5)
      DIMENSION TMAX(5)
      DIMENSION GEARV(10),GRAT(10),EBX(5),EBY(5)
      DIMENSION XX(225),YY(225),NUM(9),TQ(5,2)
      DIMENSION MAXAX(5),EMPTY(5)
      DIMENSION DCS(4,3),DJS(3,3)         ,DKT(3,3),DWS(4,3)
      DATA DCS/'REAR    ','TIRES (L','BS)     ','        ',
     1         'FRONT   ','TANDEM T','IRES (LB','S)      ',
     2         'TIRES   ','(LBS)   ','        ','        '/
      DATA DJS/'REAR WHE','ELS     ','        ',
     1         'FRONT TA','NDEM WHE','ELS     ',
     2         'WHEELS  ','        ','        '/
      DATA     DKT/'REAR TIR','ES      ','        ',
     1             'FRONT TA','NDEM TIR','ES      ',
     2             'TIRES   ','        ','        '/
      DATA DWS/'REAR SUS','PENSION ','(LBS)   ','        ',
     1         'FRONT TA','NDEM    ','SUSPENSI','ON (LBS)',
     2         'SUSPENSI','ON      ','(LBS)   ','        '/
      RADIAN = 57.295779
      WRITE (W,9500)
 9500 FORMAT(///' *** BEGIN INPUT ***')
C
C READ HEAD AND WRITE HEADER
C
      NP=1
      READ (R,200) HEAD
  200 FORMAT(20A4)
      WRITE (W,201) HEAD,NP
      NP=NP+1
  201 FORMAT('1',T52,'HSRI DYNAMIC VEHICLE SIMULATION'/
     1  T51,'TRACTOR-TRAILER TANDEM AXLE MODEL'/
     2  T28,20A4/T64,'PAGE',I3//)

C READ KEY(1)
C
      READ (R,8202) KEY(1)
C
C OPTION 1
C ROAD KEY
C IF KEY(1)=-1, SET KROAD=KEY(1) AND READ KEY(1) AGAIN
C IF KROAD = -1, SUBROUTINE ROAD WILL BE CALLED FROM FCT
C
      KROAD=0
      IF(KEY(1).NE.-1) GO TO 3800
      KROAD=KEY(1)
      READ (R,8202) KEY(1)
 8202 FORMAT(I2)
 3800 CONTINUE
C
C READ KEY(2)
C
      READ (R,8202) KEY(2)
C
C DETERMINE KEY1, KEY2, KAXLE, AND KTYPE
C
      KEY1 = KEY(1) + 1
      KEY2 = KEY(2) + 1
      KAXLE=3
      KTYPE=1
C
C READ INPUT PARAMETERS
C
      IF(KEY(1).EQ.0) GO TO 10000
      KAXLE=KAXLE+1
      KTYPE=KTYPE+1
      READ (R,203) AA1
      READ (R,203) AA2
      READ (R,203) AA4
      READ (R,203) AA5
      IF(KEY(1).EQ.1) GO TO 10000
      KTYPE=KTYPE+2
      READ (R,203) AA6
      READ (R,203) AA7
      READ (R,203) AA8
10000 CONTINUE
      IF(KEY(2).EQ.0) GO TO 10003
      KAXLE=KAXLE+1
      KTYPE=KTYPE+1
      READ (R,203) AA9
      READ (R,203) AA10
      READ (R,203) AA12
      READ (R,203) AA13
      IF(KEY(2).EQ.1) GO TO 10003
      KTYPE=KTYPE+2
      READ (R,203) AA14
      READ (R,203) AA15
      READ (R,203) AA16
10003 CONTINUE
      READ (R,203) A1
      READ (R,203) A2
      READ (R,203) A3
      READ (R,203) A4
      DO 10500 I=1,3
10500 READ (R,203) ALPHA(I)
      READ (R,203) BB
      DO 10700 I=1,6
10700 READ (R,203) C(I)
      DO 10400 I=1,3
10400 READ (R,203) CF(I)
      DO 10800 I=1,KAXLE
10800 READ (R,203) CS(I)
      READ (R,203) D
      READ (R,203) DELTA(1)
      READ (R,203) DELTA(3)
      DO 10300 I=1,KAXLE
10300 READ (R,203) FA(I)
      READ (R,203) J
      READ (R,203) J1
      DO 10200 I=1,KAXLE
10200 READ (R,203) JS(I)
      DO 10100 I=1,3
10100 READ (R,203) K(I)
      DO 10600 I=1,KAXLE
10600 READ (R,203) KT(I)
      DO 11100 I=1,KAXLE
11100 READ (R,203) MUZERO(I)
      IF(KEY(1).EQ.1) READ (R,203) PERCNT(1)
      IF(KEY(2).EQ.1) READ (R,203) PERCNT(2)
      READ (R,203) PW
C IF PW = 0.0, DO NOT READ PJ, PX, OR PZ
      IF(PW.EQ.0.0) GO TO 8000
      READ (R,203) PJ
      READ (R,203) PX
      READ (R,203) PZ
 8000 CONTINUE
      READ (R,203) TIMF
      READ (R,203) VEL
      READ (R,203) W1
      READ (R,203) W2
      DO 8001 I=1,KAXLE
 8001 READ (R,203) WS(I)
C
      IW=1
      IF(KEY(1).NE.0) IW=2
      IWT=3
      IF(KEY(2).NE.0) IWT=2
      MAXLE=IW+2
      NAXLE=IW+3
C
C ECHO PARAMETERS
C
      WRITE (W,10510)
10510 FORMAT(T6,'INPUT PARAMETER TABLE'//T6,'SYMBOL',T18,'DESCRIPTION',
     1  T56,'INITIAL VALUE')
      WRITE (W,3500) KEY(1)
      WRITE (W,557) KEY(2)
      WRITE (W,8300) KROAD
      IF(KEY(1).EQ.0) GO TO 8301
      IF(KEY(1).EQ.2) GO TO 8302
      WRITE (W,101) AA1
      WRITE (W,102) AA2
      WRITE (W,104) AA4
      WRITE (W,105) AA5
      GO TO 8301
 8302 WRITE (W,501) AA1
      WRITE (W,502) AA2
      WRITE (W,504) AA4
      WRITE (W,505) AA5
      WRITE (W,506) AA6
      WRITE (W,507) AA7
      WRITE (W,508) AA8
 8301 CONTINUE
      IF(KEY(2).EQ.0) GO TO 8303
      IF(KEY(2).EQ.2) GO TO 8304
      WRITE (W,625) AA9
      WRITE (W,513) AA10
      WRITE (W,515) AA12
      WRITE (W,516) AA13
      GO TO 8303
 8304 WRITE (W,517) AA9
      WRITE (W,518) AA10
      WRITE (W,520) AA12
      WRITE (W,521) AA13
      WRITE (W,522) AA14
      WRITE (W,523) AA15
      WRITE (W,524) AA16
 8303 CONTINUE
      WRITE (W,106) A1
      WRITE (W,107) A2
      WRITE (W,531) A3
      WRITE (W,532) A4
      WRITE (W,108) ALPHA(1)
      WRITE (W,109) ALPHA(2)
      WRITE (W,110) ALPHA(3)
      IF(KTYPE.NE.7) GO TO 9525
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9525 CONTINUE
      WRITE (W,536) BB
      WRITE (W,111) C(1)
      WRITE (W,112) C(2)
      IF(KTYPE.NE.5) GO TO 9524
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9524 CONTINUE
      WRITE (W,113) C(3)
      WRITE (W,114) C(4)
      WRITE (W,537) C(5)
      WRITE (W,538) C(6)
      IF(KTYPE.NE.4 .AND. KTYPE.NE.3) GO TO 9522
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9522 CONTINUE
      WRITE (W,115) CF(1)
      WRITE (W,146) CF(2)
      WRITE (W,539) CF(3)
      DO 1000 I=1,3
 1000 IF(CF(I).EQ.0.0) CF(I)=0.001
      IF(KTYPE.NE.2) GO TO 9521
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9521 CONTINUE
      WRITE (W,116) CS(1)
      WRITE (W,117) (DCS(IJ,IW),IJ=1,4),CS(2)
      IF(KEY(1).NE.0) WRITE (W,118) CS(3)
      WRITE (W,540) MAXLE,(DCS(IJ,IWT),IJ=1,4),CS(MAXLE)
      IF(KEY(2).NE.0) WRITE (W,541) NAXLE,CS(NAXLE)
      WRITE (W,542) D
      IF(KTYPE.NE.1) GO TO 9520
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9520 CONTINUE
      WRITE (W,119) DELTA(1)
      WRITE (W,543) DELTA(3)
      WRITE (W,121) FA(1)
      WRITE (W,8400) (DKT(IJ,IW),IJ=1,3),FA(2)
      IF(KEY(1).NE.0) WRITE (W,8401) FA(3)
      WRITE (W,8402) MAXLE,(DKT(IJ,IWT),IJ=1,3),FA(MAXLE)
      IF(KEY(2).NE.0) WRITE (W,8403) NAXLE,FA(NAXLE)
      WRITE (W,122) J
      WRITE (W,545) J1
      IF(KTYPE.NE.7) GO TO 9530
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9530 CONTINUE
      WRITE (W,123) JS(1)
      IF(KTYPE.NE.5) GO TO 9529
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9529 CONTINUE
      WRITE (W,124) (DJS(IJ,IW),IJ=1,3),JS(2)
      IF(KEY(1).NE.0) WRITE (W,125) JS(3)
      WRITE (W,546) MAXLE,(DJS(IJ,IWT),IJ=1,3),JS(MAXLE)
      IF(KEY(2).NE.0) WRITE (W,547) NAXLE,JS(NAXLE)
      IF(KTYPE.NE.3) GO TO 9528
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9528 CONTINUE
      WRITE (W,126) K(1)
      WRITE (W,127) K(2)
      WRITE (W,548) K(3)
      IF(KTYPE.NE.4) GO TO 9527
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9527 CONTINUE
      WRITE (W,3501) KT(1)
      WRITE (W,128) (DKT(IJ,IW),IJ=1,3),KT(2)
      IF(KEY(1).NE.0) WRITE (W,129) KT(3)
      WRITE (W,550) MAXLE,(DKT(IJ,IWT),IJ=1,3),KT(MAXLE)
      IF(KEY(2).NE.0) WRITE (W,551) NAXLE,KT(NAXLE)
      IF(KTYPE.NE.2) GO TO 9526
      WRITE (W,201) HEAD,NP
      NP=NP+1
 9526 CONTINUE
      WRITE (W,136) MUZERO(1)
      WRITE (W,137) (DKT(IJ,IW),IJ=1,3),MUZERO(2)
      IF(KEY(1).NE.0) WRITE (W,138) MUZERO(3)
      WRITE (W,552) MAXLE,(DKT(IJ,IWT),IJ=1,3),MUZERO(MAXLE)
      IF(KEY(2).NE.0) WRITE (W,553) NAXLE,MUZERO(NAXLE)
      IF(KEY(1).EQ.1) WRITE (W,139) PERCNT(1)
      IF(KEY(2).EQ.1) WRITE (W,558) PERCNT(2)
      WRITE (W,8410) PW
      IF(PW.EQ.0.0) GO TO 30000
      WRITE (W,8411) PJ
      WRITE (W,8412) PX
      WRITE (W,8413) PZ
30000 CONTINUE
      WRITE (W,141) TIMF
      WRITE (W,140) VEL
      WRITE (W,132) W1
      WRITE (W,133) W2
      WRITE (W,134) WS(1)
      WRITE (W,135) (DWS(IJ,IW),IJ=1,4),WS(2)
      IF(KEY(1).GT.0) WRITE (W,554) WS(3)
      WRITE (W,555) MAXLE,(DWS(IJ,IWT),IJ=1,4),WS(MAXLE)
      IF(KEY(2).NE.0) WRITE (W,556) NAXLE,WS(NAXLE)
C
C **  FORMATS
C
 3500 FORMAT(T6,'KEY(1)',T18,'TRACTOR AXLE KEY: 0 FOR SINGLE AXLE'/
     1  T36,'1 FOR WALKING BEAM'/T36,'2 FOR 4 ELLIPTIC LEAF',T60,I10)
  557 FORMAT(T6,'KEY(2)',T18,'TRAILER AXLE KEY',T60,I10)
 8300 FORMAT(T6,'KROAD',T18,'ROAD KEY: 0 IMPLIES A SMOOTH ROAD'/
     1  T28,'-1 IMPLIES A ROUGH ROAD',T60,I10)
  511 FORMAT(T18,'TRACTOR TANDEM AXLE:')
  101 FORMAT(T6,'AA1',T18,'HORIZONTAL DISTANCE FROM TRACTOR WALKING'/
     1  T18,'BEAM TO FRONT TANDEM AXLE (IN)',T60,F10.2)
  102 FORMAT(T6,'AA2',T18,'HORIZONTAL DISTANCE FROM TRACTOR WALKING'/
     1  T18,'BEAM TO REAR TANDEM AXLE (IN)',T60,F10.2)
  104 FORMAT(T6,'AA4',T18,'VERTICAL DISTANCE FROM AXLE TO TRACTOR'/
     1  T18,'WALKING BEAM (IN)',T60,F10.2)
  105 FORMAT(T6,'AA5',T18,'VERTICAL DISTANCE FROM AXLE TO TRACTOR'/
     1  T18,'TORQUE ROD (IN)',T60,F10.2)
  501 FORMAT(T6,'AA1',T18,'HORIZONTAL DISTANCE FORM TRACTOR FRONT'/
     1  T18,'LEAF-FRAME CONTACT TO AXLE CENTER (IN)',T60,F10.2)
  502 FORMAT(T6,'AA2',T18,'HORIZONTAL DISTANCE FROM TRACTOR REAR'/
     1  T18,'LEAF-FRAME CONTACT TO AXLE CENTER (IN)',T60,F10.2)
  504 FORMAT(T6,'AA4',T18,'HORIZONTAL DISTANCE FROM TRACTOR FRONT'/
     1  T18,'LEAF-FRAME CONTACT TO LOAD LEVER PIN (IN)',T60,F10.2)
  505 FORMAT(T6,'AA5',T18,'HORIZONTAL DISTANCE FROM TRACTOR REAR'/
     1  T18,'LEAF-FRAME CONTACT TO LOAD LEVER PIN (IN)',T60,F10.2)
  506 FORMAT(T6,'AA6',T18,'VERTICAL DISTANCE FROM AXLE DOWN TO '/
     1  T18,'TRACTOR TORQUE ROD (IN)',T60,F10.2)
  507 FORMAT(T6,'AA7',T18,'ANGLE BETWEEN TRACTOR TORQUE ROD AND'/
     1  T18,'HORIZONTAL (DEG)',T60,F10.2)
  508 FORMAT(T6,'AA8',T18,'HORIZONTAL DISTANCE FROM AXLE CENTER'/
     1  T18,'FORWARD TO TRACTOR TORQUE ROD (IN)',T60,F10.2)
  625 FORMAT(T6,'AA9',T18,'HORIZONTAL DISTANCE FROM TRAILER WALKING'/
     1  T18,'BEAM TO FRONT TANDEM AXLE (IN)',T60,F10.2)
  513 FORMAT(T6,'AA10',T18,'HORIZONTAL DISTANCE FROM TRAILER WALKING'/
     1  T18,'BEAM TO REAR TANDEM AXLE (IN)',T60,F10.2)
  515 FORMAT(T6,'AA12',T18,'VERTICAL DISTANCE FROM AXLE TO TRAILER'/
     1  T18,'WALKING BEAM (IN)',T60,F10.2)
  516 FORMAT(T6,'AA13',T18,'VERTICAL DISTANCE FROM AXLE TO TRAILER'/
     1  T18,'TORQUE ROD (IN)',T60,F10.2)
  517 FORMAT(T6,'AA9',T18,'HORIZONTAL DISTANCE FROM TRAILER FRONT'/
     1  T18,'LEAF-FRAME CONTACT TO AXLE CENTER (IN)',T60,F10.2)
  518 FORMAT(T6,'AA10',T18,'HORIZONTAL DISTANCE FROM TRAILER REAR'/
     1  T18,'LEAF-FRAME CONTACT TO AXLE CENTER (IN)',T60,F10.2)
  520 FORMAT(T6,'AA12',T18,'HORIZONTAL DISTANCE FROM TRAILER FRONT'/
     1  T18,'LEAF-FRAME CONTACT TO LOAD LEVER PIN (IN)',T60,F10.2)
  521 FORMAT(T6,'AA13',T18,'HORIZONTAL DISTANCE FROM TRAILER REAR'/
     1  T18,'LEAF-FRAME CONTACT TO LOAD LEVER PIN (IN)',T60,F10.2)
  522 FORMAT(T6,'AA14',T18,'VERTICAL DISTANCE FROM AXLE DOWN TO'/
     1  T18,'TRAILER TORQUE ROD (IN)',T60,F10.2)
  523 FORMAT(T6,'AA15',T18,'ANGLE BETWEEN TRAILER TORQUE ROD AND'/
     1  T18,'HORIZONTAL (DEG)',T60,F10.2)
  524 FORMAT(T6,'AA16',T18,'HORIZONTAL DISTANCE FROM AXLE CENTER'/
     1  T18,'FORWARD TO TRAILER TORQUE ROD (IN)',T60,F10.2)
  106 FORMAT(T6,'A1',T18,'HORIZONTAL DISTANCE FROM TRACTOR CG TO'/
     1  T18,'CENTER OF TRACTOR FRONT SUSPENSION (IN)',T60,F10.2)
  107 FORMAT(T6,'A2',T18,'HORIZONTAL DISTANCE FROM TRACTOR CG TO'/
     1  T18,'CENTER OF TRACTOR REAR SUSPENSION (IN)',T60,F10.2)
  531 FORMAT(T6,'A3',T18,'HORIZONTAL DISTANCE FROM TRAILER CG TO'/
     1  T18,'5TH WHEEL (IN)',T60,F10.2)
  532 FORMAT(T6,'A4',T18,'HORIZONTAL DISTANCE FROM TRAILER CG TO'/
     1  T18,'CENTER OF TRAILER SUSPENSION (IN)',T60,F10.2)
  108 FORMAT(T6,'ALPHA1',T18,'STATIC DISTANCE, TRACTOR FRONT AXLE TO'/
     1  T18,'GROUND (IN)',T60,F10.2)
  109 FORMAT(T6,'ALPHA2',T18,'STATIC DISTANCE, TRACTOR REAR AXLE(S) TO'/
     1  T18,'GROUND (IN)',T60,F10.2)
  110 FORMAT(T6,'ALPHA3',T18,'STATIC DISTANCE, TRAILER AXLE(S) TO '/
     1  T18,'GROUND (IN)',T60,F10.2)
  536 FORMAT(T6,'BB',T18,'HORIZONTAL DISTANCE FROM 5TH WHEEL TO MIDPOINT
     1  '/T18,'OF TRACTOR REAR SUSPENSION (IN)',T60,F10.2)
  111 FORMAT(T6,'C1',T18,'VISCOUS DAMPING: JOUNCE ON TRACTOR FRONT'/
     1  T18,'SUSPENSION (LB-SEC/IN)',T60,F10.2)
C ENGINE BRAKING AND/OR PROP SHAFT BRAKING AND/OR AUXILIARY RETARDERS
C COMMON /BLOCK3/ ICOUNT,NOGEAR,GEARV,GRAT,NUMEB,EBX,EBY,EBPER,EMINUS
  112 FORMAT(T6,'C2',T18,'VISCOUS DAMPING: REBOUND ON TRACTOR FRONT'/
     1  T18,'SUSPENSION (LB-SEC/IN)',T60,F10.2)
  113 FORMAT(T6,'C3',T18,  'VISCOUS DAMPING: JOUNCE ON TRACTOR REAR'/
     1  T18,'SUSPENSION (LB-SEC/IN)',T60,F10.2)
  114 FORMAT(T6,'C4',T18,'VISCOUS DAMPING: REBOUND ON TRACTOR REAR'/
     1  T18,'SUSPENSION (LB-SEC/IN)',T60,F10.2)
  537 FORMAT(T6,'C5',T18,'VISCOUS DAMPING: JOUNCE ON TRAILER'/T18,
     1  'SUSPENSION (LB-SEC/IN)',T60,F10.2)
  538 FORMAT(T6,'C6',T18,'VISCOUS DAMPING: REBOUND ON TRAILER'/T18,
     1  'SUSPENSION (LB-SEC/IN)',T60,F10.2)
  115 FORMAT(T6,'CF1',T18,'MAXIMUM COULOMB FRICTION, TRACTOR FRONT'/
     1  T18,'SUSPENSION (LB)',T60,F10.2)
  146 FORMAT(T6,'CF2',T18,'MAXIMUM COULOMB FRICTION, TRACTOR REAR'/
     1  T18,'SUSPENSION (LB)',T60,F10.2)
  539 FORMAT(T6,'CF3',T18,'MAXIMUM COULOMB FRICTION, TRAILER'/T18,
     1  'SUSPENSION (LB)',T60,F10.2)
  116 FORMAT(T6,'CS1',T18,'LONGITUDINAL STIFFNESS, TRACTOR FRONT     '/
     1  T18,'TIRES (LBS)',T60,F10.2)
  117 FORMAT(T6,'CS2',T18,'LONGITUDINAL STIFFNESS, TRACTOR ',A8/T18,3A8,
     1  T60,F10.2)
  118 FORMAT(T6,'CS3',T18,'LONGITUDINAL STIFFNESS, TRACTOR REAR      '/
     1  T18,'TANDEM TIRES (LBS)',T60,F10.2)
  540 FORMAT(T6,'CS',I1,T18,'LONGITUDINAL STIFFNESS, TRAILER ',A8/
     1  T18,3A8,T60,F10.2)
  541 FORMAT(T6,'CS',I1,T18,'LONGITUDINAL STIFFNESS, TRAILER REAR'/
     1  T18,'TANDEM TIRES (LBS)',T60,F10.2)
  542 FORMAT(T6,'D',T18,  'VERTICAL DISTANCE FROM 5TH WHEEL          '/
     1  T18,'CONNECTION DOWN TO TRACTOR CG (IN)',T60,F10.2)
  119 FORMAT(T6,'DELTA1',T18,'STATIC VERTICAL DISTANCE, TRACTOR CG TO'/
     1  T18,'TRACTOR FRONT AXLE (IN)',T60,F10.2)
  543 FORMAT(T6,'DELTA3',T18,'STATIC VERTICAL DISTANCE, TRAILER CG TO'/
     1  T18,'TRAILER AXLE(S) (IN)',T60,F10.2)
  121 FORMAT(T6,'FA1',T18,'FRICTION REDUCTION PARAMETER FOR TRACTOR'/T18
     1  ,'FRONT TIRES',T60,F10.3)
 8400 FORMAT(T6,'FA2',T18,'FRICTION REDUCTION PARAMETER FOR TRACTOR'/T18
     1  ,3A8,T60,F10.3)
 8401 FORMAT(T6,'FA3',T18,'FRICTION REDUCTION PARAMETER FOR TRACTOR'/T18
     1  ,'REAR TANDEM TIRES',T60,F10.3)
 8402 FORMAT(T6,'FA',I1,T18,'FRICTION REDUCTION PARAMETER FOR TRAILER'/
     1  T18,3A8,T60,F10.3)
 8403 FORMAT(T6,'FA',I1,T18,'FRICTION REDUCTION PARAMETER FOR TRAILER'/
     1  T18,'REAR TANDEM TIRES',T60,F10.3)
  122 FORMAT(T6,'J',T18,'TRACTOR POLAR MOMENT (IN-LB-SEC**2)',T55,F15.2)
  545 FORMAT(T6,'J1',T18,'TRAILER POLAR MOMENT OF INERTIA'/T18,
     1  '(IN-LB-SEC**2)',T55,F15.2)
  123 FORMAT(T6,'JS1',T18,'POLAR MOMENT OF TRACTOR FRONT WHEELS'/T18,
     1  '(IN-LB-SEC**2)',T55,F15.2)
  124 FORMAT(T6,'JS2',T18,'POLAR MOMENT OF TRACTOR ',3A8/T18,
     1  '(IN-LB-SEC**2)',T55,F15.2)
  125 FORMAT(T6,'JS3',T18,'POLAR MOMENT OF TRACTOR REAR TANDEM WHEELS'/
     1  T18,'(IN-LB-SEC**2)',T55,F15.2)
  546 FORMAT(T6,'JS',I1,T18,'POLAR MOMENT OF TRAILER ',3A8/T18,
     1  '(IN-LB-SEC**2)',T55,F15.2)
  547 FORMAT(T6,'JS',I1,T18,'POLAR MOMENT OF TRAILER REAR TANDEM WHEELS'
     1  /T18,'(IN-LB-SEC**2)',T55,F15.2)
  126 FORMAT(T6,'K1',T18, 'SPRING RATE, TRACTOR FRONT SUSPENSION'/T18,
     1  '(LB/IN)',T60,F10.2)
