      GO TO 6421
 7002 WRITE (W,6420)
 6420 FORMAT('0ENGINE BRAKING IS NOT IN EFFECT THIS RUN')
 6421 CONTINUE
      IF(KPSB.EQ.0) GO TO 6412
      WRITE (W,6413)
 6413 FORMAT(' PROP SHAFT BRAKING WILL BE USED')
      GO TO 9802
 6412 WRITE (W,9803)
