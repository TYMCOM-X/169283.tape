     1  T15,'VELOCITY (FT/SEC) VS GEAR')
      DO 8201 I=1,NOGEAR
 8201 WRITE (W,8700) GEARV(I),I
 8700 FORMAT(T6,F12.4,2X,I10)
      WRITE (W,6410)
 6410 FORMAT('0',T6,'TABLE II: TRANSMISSION TABLE'/
     1  T16,'GEAR VS GEAR RATIO')
      DO 8203 I=1,NOGEAR
