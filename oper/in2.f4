 9602 WRITE (W,9604)
 9604 FORMAT(T15,'TRACTOR FRONT TANDEM BRAKES')
      GO TO 3000
 3004 CONTINUE
      WRITE (W,9605)
 9605 FORMAT(T15,'TRACTOR REAR TANDEM BRAKES')
      GO TO 3000
 3011 CONTINUE
      IF(KEY(2).EQ.0) GO TO 9606
      WRITE (W,9607)
 9607 FORMAT(T15,'TRAILER FRONT TANDEM BRAKES')
      GO TO 3000
 9606 WRITE (W,9608)
 9608 FORMAT(T15,'TRAILER BRAKES')
