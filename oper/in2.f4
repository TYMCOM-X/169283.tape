C
      LAH=JAXLE
      DO 4015 I=7,9
      I6=I-6
      IF(K(I6).GE.0.0) GO TO 4015
      LINE=LINE+NUM(I)+6
      IF(LINE.LE.45) GO TO 3613
      WRITE (W,201) HEAD,NP
      NP=NP+1
      LINE=NUM(I)+6
 3613 CONTINUE
      LAH=LAH+1
      KKF=(I-1)*25+NUM(I)
      KKKF=(I-1)*25+1
      WRITE (W,9611) LAH
 9611 FORMAT('0',T6,'TABLE',I2,': SPRING DEFLECTION (IN) VS FORCE')
      GO TO (9612,9613,9614), I6
 9612 WRITE (W,9615)
 9615 FORMAT(T15,'TRACTOR FRONT SUSPENSION')
      GO TO 3605
 9613 IF(KEY(1).GT.0) GO TO 9616
      WRITE (W,9617)
 9617 FORMAT(T15,'TRACTOR REAR SUSPENSION')
      GO TO 3605
 9616 WRITE (W,8704)
 8704 FORMAT(T15,'TRACTOR REAR SUSPENSION  ...FOR ONE OF THE AXLES'/
     1  T15,'NOTE: BOTH AXLES HAVE IDENTICAL SPRINGS')
      GO TO 3605
 9614 IF(KEY(2).GT.0) GO TO 9619
      WRITE (W,9620)
 9620 FORMAT(T15,'TRAILER SUSPENSION')
      GO TO 3605
 9619 WRITE (W,9621)
 9621 FORMAT(T15,'TRAILER SUSPENSION  ...FOR ONE OF THE AXLES'/
     1  T15,'NOTE: BOTH AXLES HAVE IDENTICAL SPRINGS')
 3605 WRITE (W,9610) NUM(I),(XX(IJ),YY(IJ),IJ=KKKF,KKF)
73605 CONTINUE
      DO 4016 KI=KKKF,KKF
 4016 XX(KI)=XX(KI)/12.
