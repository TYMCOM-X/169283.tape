 9802 CONTINUE
      IF(KEYAR.EQ.0) GO TO 9804
      WRITE (W,9805)
 9805 FORMAT(' AUXILIARY RETARDERS WILL BE USED')
      GO TO 9806
 9804 WRITE (W,9807)
 9807 FORMAT(' AUXILIARY RETARDERS WILL NOT BE USED')
 9806 CONTINUE
      IF(ICOUNT.EQ.0) GO TO 6422
      WRITE (W,6409)
