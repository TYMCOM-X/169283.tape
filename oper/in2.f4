      KPROP=0
      IF(NUM(1).NE.-1) GO TO 3920
      KPROP = NUM(1)
      DO 3921 I=1,KAXLE
 3921 READ (R,204) MAXAX(I)
      DO 13921 I=1,KAXLE
13921 READ (R,204) EMPTY(I)
      READ (R,202) NUM(1)
 3920 CONTINUE
