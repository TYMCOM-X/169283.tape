10 FILES BUG.BUG$,TWO.DAT$90
15 DIM M$(10)
20 SET:1,1
30 FOR I =1 TO 6
40 READ :1: L$(I)
50  PRINT LEN(L$(I))
55 PRINT L$(I)
60 NEXT I
110 FOR X=1 TO 500
120 SET :2, 1
130 FOR I=1 TO 6
140 READ :2: M$(I)
150 IF M$(I)=L$(I) GO TO 300
160 PRINT "ERR" ;X;I
170 PRINT "M$",M$(I)
180 PRINT "L$",L$(I)
300 NEXT I
400 NEXT X
500 END
