
! ;THE COMPONENTS OF THE MODIFIED BNF SYNTAX USED IN THE PRODUCTIONS ON THE FOLLOWING PAGES .
! 
! BNFDEFINITION      = METASYMBOL "="  BNFCOMPONENT  [ COMMENT ] "." .
! 
! BNFCOMPONENT       = ( TERMINAL l
!                      METASYMBOL l
!                      ONE l
!                      ALL l
!                      OPTION l
!                      LIST l
!                      REPEAT l
!                      ACTION ) .
! 
! TERMINAL           = """ *ALPHANUMERIC """ ;TERMINAL CHARACTER STRING .
! 
! METASYMBOL         = [ *LETTER  "-" ] *LETTER  ;AN ELEMENT
!                      OF THE SYNTAX DEFINED BY THE SYNTAX OR BY A LEXICAL
!                      PROCESS .
! 
! ONE                = "(" BNFCOMPONENT *[ "l" BNFCOMPONENT ] ")" 
!                      ;ONLY ONE OF THE BNFCOMPONENTS IS REQUIRED .
! 
! ALL                = BNFCOMPONENT *[ BNFCOMPONENT ] ;ALL OF THE
!                      BNFCOMPONENTS ARE REQUIRED .
! 
! OPTION             = "[" BNFCOMPONENT "]" ;USE OF THE BNFCOMPONENT IS
!                      OPTIONAL .
! 
! LIST               = "+" BNFCOMPONENT ;BNFCOMPONENTS SEPERATED BY
!                      COMMAS .
! 
! REPEAT             = "*" BNFCOMPONENT ;BNFCOMPONENTS NOT SEPERATED BY
!                      COMMAS .
! 
! ACTION             = "%" ACTIONNAME "%" ;ACTION TO BE PERFORMED WHEN ENCOUNTERED .
! 
! COMMENT            = ";" *ASCIICHARACTER .
! 
! ;*****************************************************************************************************************
! 
! ;FORTRAN-10 SYNTAX IN MODIFIED BNF .
! 
! EXPRESSION     = %EXPRESS% .
! 
! VARIABLESPEC       = IDENTIFIER [ LPAREN +%EXPRESS% RPAREN ] [ FIELDSPEC ] .
! 
! FIELDSPEC          = LBRKT ( CONSTANT l IDENTIFIER ) COLON ( CONSTANT l IDENTIFIER ) RBRKT .
! 
! DATA               = +( +DATAITEM VALUELIST ) LINEND .
! 
! ;FORMAT             = %FORMATSYN% .
! 
! SUBSCRIPTINTEGER   = DECLARESPEC LINEND .
! 
! INTEGERSPEC        = DECLARESPEC LINEND .
! 
! REALSPEC           = DECLARESPEC LINEND .
! 
! LOGICALSPEC        = DECLARESPEC LINEND .
! 
! DOUBLEPRECISION    = DECLARESPEC LINEND .
! 
! COMPLEXSPEC        = DECLARESPEC LINEND .
! 
! BYTESPEC           = DECLARESPEC LINEND .
! 
! DECLARESPEC        = ( FUNCTIONSPEC l TYPELIST ) .
! 
! FUNCTIONSPEC       = %FUNCTIONSCAN% SUBPROGRAMID .
! 

! SUBROUTINE         = %FIRSTSTATEMENT% SUBPROGRAMID LINEND .
! 
! ENTRYSPEC          = SUBPROGRAMID LINEND .
! 
! PROGRAM            = %FIRSTSTATEMENT% [ IDENTIFIER ] LINEND .
! 
! BLOCKDATA          = %FIRSTSTATEMENT% [ IDENTIFIER ] LINEND .
! 
! DIMENSION          = +ONEARRAY LINEND .
! 
! COMMON             = *COMMONGROUP LINEND .
! 
! EQUIVALENCE        = +( LPAREN VARIABLESPEC COMMA +VARIABLESPEC RPAREN ) LINEND .
! 
! EXTERNSPEC         = +ONEARRAY LINEND .
! 
! INCLUDESPEC        = LPAREN [ UNITSPEC COMMA ] ( LITSTRING l VARIABLESPEC ) RPAREN LINEND .
! 
! IMPLICIT           = +( %TYPEID% LPAREN +IMPLICITSPEC RPAREN ) LINEND .
! 
! PROTECT            = +ONEARRAY LINEND .
! 
! GLOBALSPEC         = +ONEARRAY LINEND .
! 
! NAMELIST           = *( SLASH IDENTIFIER SLASH +IDENTIFIER ) LINEND .
! 
! IMPLICITSPEC       = %LETTER% [ DASH %LETTER% ] .
!  
! ;TYPEID             = ( "SUBSCRIPTINTEGER" l "DOUBLEPRECISION" l
! ;                     ( "INTEGER" l "REAL" l "LOGICAL" l "COMPLEX" l "BIT" ) [ TIMES CONSTANT ] ) %SETTYPE% .
! 
! DATAITEM           = (  VARIABLESPEC l  LPAREN +DATAITEM [ LOOPPART ] RPAREN )  .
! 
! LOOPPART           = EQUAL EXPRESSION COMMA EXPRESSION [ COMMA EXPRESSION ] .
! 
! SUBPROGRAMID       = IDENTIFIER [ LPAREN +( IDENTIFIER l DOLLAR l ASTERISK ) RPAREN ]  .
! 
! ONEARRAY           = IDENTIFIER [ [ * ( COLON IDENTIFIER ) ]
!                      LPAREN +( [ PLUS l MINUS ] ( CONSTANT l IDENTIFIER ) [ SLASH [ PLUS l MINUS ] ( CONSTANT l IDENTIFIER ) ] ) 
!                      RPAREN ] .
! 
! TYPELIST           = +( +ONEARRAY  [ VALUELIST ] ) .
! 
! VALUELIST          = SLASH +([ PLUS l MINUS ] CONSTANT
!                      [ TIMES ([ PLUS l MINUS ] CONSTANT l LITSTRING l CMPLEXCON ) ] l LITSTRING l CMPLEXCON ) SLASH . 
! 
! CMPLEXCON          = LPAREN [ PLUS l MINUS ] CONSTANT COMMA [ PLUS l MINUS ] CONSTANT RPAREN .
! 

! COMMONGROUP        = [ SLASH [ IDENTIFIER ] SLASH ] +ONEARRAY .
! 
! ASSIGN             = CONSTANT %TOQUOTE% VARIABLESPEC LINEND .
! 
! GOTO               = ( CONSTANT l GOTOSPEC ) LINEND .
! 
! CALL               = IDENTIFIER [ LPAREN %EXPRLIST% RPAREN ] LINEND .
! 
! RETURNSPEC         = [ EXPRESSION ] LINEND .
! 
! ;CONTINUE          = LINEND .
! 
! STOP               = [ LITSTRING l CONSTANT ] LINEND .
! 
! PAUSE              = [ LITSTRING l CONSTANT ] LINEND .
! 
! ;END               = LINEND .
! 
! INPUT              = [ LPAREN ( CONSTANT l VARIABLESPEC ) RPAREN ] [ COMMA ]  +DATAITEM LINEND .
! 
! READSPEC           = ( IOSPEC l FORMATID ) [ [ COMMA ]  +DATAITEM ] LINEND .
! 
! ACCEPT             = FORMATID [ COMMA +DATAITEM ] LINEND .
! 
! PUNCH              = FORMATID [ COMMA +DATAITEM ] LINEND .
! 
! WRITESPEC          = ( IOSPEC l FORMATID ) [ [ COMMA ] +DATAITEM ] LINEND .
! 
! OUTPUT             = [ LPAREN ( CONSTANT l VARIABLESPEC ) RPAREN ] [ COMMA ]  +OUTPUTITEM LINEND .
! 
! PRINT              = FORMATID [ COMMA  +DATAITEM ] LINEND .
! 
! TYPESPEC           = FORMATID [ COMMA  +DATAITEM ] LINEND .
! 
! ENCODE             = ENCODECODESPEC LINEND .
! 
! DECODE             = ENCODECODESPEC LINEND .
! 
! REREAD             = FORMATID [ COMMA +DATAITEM ] LINEND .
! 

! BACKSPEC           = REPEATEXPR LINEND ;(BACKSPACE l BACKFILE ).
! 
! REWIND             = UTILSPEC .
! 
! UNLOAD             = UTILSPEC .
! 
! ENDFILESPEC        = UTILSPEC .
! 
! SKIPSPEC           = REPEATEXPR LINEND ;(SKIPRECORD l SKIPFILE ) .
! 
! ;OPEN               = LPAREN +( IDENTIFIER EQUAL ( VARIABLESPEC l CONSTANT l LITSTRING ) ) RPAREN LINEND .
! 
! ;CLOSESPEC          = LPAREN ( CONSTANT l VARIABLESPEC )
! ;                     *[ COMMA IDENTIFIER EQUAL ( VARIABLESPEC l LITSTRING ) ] RPAREN LINEND .
! 
! FIND               = LPAREN ( CONSTANT l IDENTIFIER ) %RECORDMARK% EXPRESSION RPAREN LINEND .
! 
! IOSPEC             = LPAREN UNITSPEC [ COMMA +( FORMATID [ EQUAL ( CONSTANT l VARIABLESPEC ) ] ) ] RPAREN  .
! 
! GOTOSPEC           = ( VARIABLESPEC [ [ COMMA ] LPAREN +CONSTANT RPAREN ] l
!                      LPAREN +CONSTANT RPAREN [ COMMA ] EXPRESSION ) .
! 
! ENCODECODESPEC     = LPAREN EXPRESSION COMMA FORMATID COMMA VARIABLESPEC RPAREN +DATAITEM .
! 
! FORMATID           = ( CONSTANT l IDENTIFIER l LITSTRING l ASTERISK ) .
! 
! ACTUALARG          = ( EXPRESSION l DOLLAR CONSTANT ) .
! 
! UNITSPEC           = ( CONSTANT l IDENTIFIER ) [ %RECORDMARK% EXPRESSION ] .
! 
! OUTPUTITEM         = ( %OUTPUTEXPR% l LPAREN +OUTPUTITEM [ LOOPPART ] RPAREN ) .
! 
! REPEATEXPR         = +( [ LPAREN ] ( CONSTANT l VARIABLESPEC ) [ RPAREN ] [ LPAREN ( CONSTANT l VARIABLESPEC ) RPAREN ] ) .
! 
! UTILSPEC           = [ LPAREN ] +( CONSTANT l VARIABLESPEC ) [ RPAREN ] LINEND .
! 

! STATEFUNCSPEC      = LPAREN %LOCALARGLIST% RPAREN  EQUAL EXPRESSION LINEND .
! 
! ARRAYASSIGNSPEC    = LPAREN +EXPRESSION RPAREN [ FIELDSPEC ] *( EQUAL EXPRESSION ) LINEND .
! 
! LOGICALIFSPEC      = EXPRESSION RPAREN .
! 
! ARITHIFSPEC        = EXPRESSION RPAREN CONSTANT COMMA CONSTANT [ COMMA CONSTANT ] LINEND .
! 
! ASSIGNSPEC         = IDENTIFIER [ FIELDSPEC ] *( EQUAL EXPRESSION ) LINEND .
! 
! DOSPEC             = %DOLABEL% IDENTIFIER EQUAL EXPRESSION COMMA EXPRESSION [ COMMA EXPRESSION ] .
! 

