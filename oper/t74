1 * T74 CONVT  SYM READ WITH BIN WRITE   CLOBBERS INPUT
2 STRING TRANS(9),CRVCF(9)
3 DATA TRANS/'TRANS'/,CRVCF/'CRVCF.RN'/
4 OPEN(3,TRANS,INPUT,SYMBOLIC)
5 OPEN(4,CRVCF,RANDOUT,BINARY)
6 X=3.7
7 DO 50 IREC=1,5
8 READ(3) A,B,C,I,J
9 WRITE(4@IREC)A,B,C,I,J
10 R=A*(X*X)+B*X+C
11 50 WRITE(1,110) IREC,R
12 110 FORMAT(I6,F12.6)
13 CLOSE(3);CLOSE(4)
14 STOP
15 END
