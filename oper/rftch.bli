MODULE RFTCH(DOLLAR,ENTRIES=($RFTCH),FSAVE,TIMING,TIMER=EXTERNAL(SIX12))=

BEGIN

REQUIRE RCDEFS.BLI[7,107355];
REQUIRE RSDEFS.BLI[7,107355];
REQUIRE RDEFS.BLI[7,107355];
REQUIRE UDEFS.BLI[7,107355];
UNDECLARE $RFTCH;

GLOBAL ROUTINE $RFTCH(RCB,ITEMND,FIAD)=

   BEGIN
   MAP RCB$ RCB;
   LOCAL RSB$ RSB;

   RSB_.RCB[RC$RSB];

      $UMOVE(.RSB[RS$SIZE,.ITEMND],.RCB[RC$RIGHT]+.RSB[RS$OFFSET,.ITEMND],.FIAD);
   END;
END ELUDOM
