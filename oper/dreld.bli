REQUIRE SDEFS.BLI[7,107355];

UNDECLARE $DRELD;

BIND   MOVE = #200,
       MOVEI = #201,
       JUMPE = #322,
       JUMPN = #326,
       JRST = #254,
       PUSH = #261,
