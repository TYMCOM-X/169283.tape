MODULE ERRO(DOLLAR, ENTRIES=($COUTF), FSAVE,TIMING,
                        TIMER=EXTERNAL(SIX12) )=
BEGIN

REQUIRE KDEFS.BLI[7,107355];
REQUIRE CDEFS.BLI[7,107355];
REQUIRE FDEFS.BLI[7,107355];

UNDECLARE $COUTF;


GLOBAL ROUTINE $COUTF(BUF)=
  BEGIN
  LOCAL KFCB$ FCB;
  FCB_.CFLCB$[CV$ERROROUT];
  FCB[K$DCNT]_FV$SZTOK;
  FCB[K$DADR]_.BUF;
  $KWRITE(FCB);
  CACTI$[CV$ERROROUT]_.CACTI$[CV$ERROROUT]+1;
  RETURN FV$SZTOK
  END;

%THIS ROUTINE WILL EVENTUALLY BE EXPANDED TO ALLOW FOR
 TRANSFER OF TOKENS TO CORE AS AN ALTERNATIVE TO DISK WHEN
 "FILE" SIZE PERMITS.
%

END
ELUDOM
