TITLE CHKAUN.MAC - CHECK AUN OF USER RUNNING THIS PROGRAM
	ENTRY CHKAUN
	INTERNAL CHKAUN
EXTERNAL OKAUN,INDEX,TSTAUN
JOBAUN==-23
CHKAUN:	0
	MOVE	1,INDEX		;NO. OF AUN'S
	HRROI	0,JOBAUN	;GETTAB,THIS JOB
	GETTAB	0,
	JRST [TTCALL 3,[ASCIZ/?/]
	CALLI 12]
CHKUSR:	SOJL	1,CHKTST	;DONE ALL BUT TEST AUN?
	CAMN	0,OKAUN(1)
	;RIGHT USER
	JRA	16,(16)	;YES
	JRST CHKUSR
CHKTST:	CAMN	0,TSTAUN	;TESTING AUN
	JRA	16,(16)
	TTCALL 3,[ASCIZ/?/]
	CALLI 12

	END
