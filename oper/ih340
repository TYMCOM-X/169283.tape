017700 INPUT-OUTPUT SECTION.                                            017400
017800 FILE-CONTROL.                                                    017500
017900     SELECT PERS-MSTR-IN-FILE ASSIGN TO RCA-3485 PRIMING-CODE 1   017600
018000        FOR MULTIPLE REEL.                                        017700
018100     SELECT PAYROLL-DETAIL-FILE ASSIGN TO RCA-3485 PRIMING-CODE 1 017800
018200        FOR MULTIPLE REEL.                                        017900
018300     SELECT PERS-MSTR-OUT-FILE ASSIGN TO RCA-3485 PRIMING-CODE 1  018000
018400        FOR MULTIPLE REEL.                                        018100
018500     SELECT PAYROLL-WORK-FILE ASSIGN TO RCA-3485 PRIMING-CODE 1   018200
018600        FOR MULTIPLE REEL.                                        018300
018700     SELECT SMOOTH-ROLL-FILE ASSIGN TO RCA-3485 PRIMING-CODE 1    018400
018800        FOR MULTIPLE REEL.                                        018500
018900     SELECT CONTROL-ERROR-FILE,                                   018600
019000        ASSIGN TO RCA-3485 PRIMING-CODE 1 FOR MULTIPLE REEL.      018700
019100     SELECT PAY-EXCEPT-LIST-FILE ASSIGN TO RCA-3485               018800
019200        PRIMING-CODE 1 FOR MULTIPLE REEL.                         018900
