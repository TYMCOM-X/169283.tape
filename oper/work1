CANOVA        SUBROUTINE ANOVA FOR BMD03V         AUGUST 17, 1966       V0302900
C                                                                       *LMSC
C            CONVERTED FOR LMSC UNIVAC 1108 BY DEPT 84-72               *LMSC
C                   LEW BUTLER   DECEMBER 1967                          *LMSC
C                                                                       *LMSC
      SUBROUTINE ANOVA                                                  V0302910
      DIMENSION X(1500,9),SCALE(9),AV(9),L(6),LL(6),N(6),M(6),MM(7),    V0302920
     1K5(7),KM(6),S(65,9,9),T(9,9),PQ(64),A(64),B(64),R(64,8),KV(9),    V0302930
     2KW(9),CON(8),NEWA(64),LCODE(64),LVA(64),BNEW(64)                  *LMSC
      COMMON /A/R                                                         V0302950
      COMMON /B/X      , SCALE  , AV     , L      , LL     , N            V0302960
      COMMON /C/M      , MM     , K5     , KM     , S      , T            V0302970
      COMMON /D/PQ     , NV     , NR     , NT     , NI     , NVM          V0302980
      COMMON /E/I5     , MARK   , JW     , Z      , IJK    , KK           V0302990
      COMMON /F/KZ     , NN     , ON     , NW     , A      , B            V0303000
      COMMON /G/KV     , KW     , NY     , NZ     , CON    , ONNN         V0303010
        DOUBLE PRECISION ON
      EQUIVALENCE (R(1),BNEW(1)),(R(65),NEWA),(R(129),LVA),             V0303020
     1(R(193),LCODE)                                                    V0303030
C     DATA Q000HL/6H(     /                                             V0303040
C     BNEW(1)=(+Q000HL)                                                 V0303050
C     DATA Q001HL/6H1H    /                                             V0303060
C     BNEW(2)=(+Q001HL)                                                 V0303070
C     DO 250 I=3,8                                                      V0303080
C     DATA Q002HL/6H1X,   /                                             V0303090
C250  BNEW(I)=(+Q002HL)                                                 V0303100
C     DATA Q003HL/6H9X,I8,/                                             V0303110
C     BNEW(9)=(+Q003HL)                                                 V0303120
C     DATA Q004HL/6HF22.5,/                                             V0303130
C     BNEW(10)=(+Q004HL)                                                V0303140
C     DATA Q005HL/6HF17.5,/                                             V0303150
C     BNEW(11)=(+Q005HL)                                                V0303160
C     BNEW(11)=(+Q005HL)                                                V0303160
C     DATA Q006HL/6H)     /                                             V0303170
C     BNEW(12)=(+Q006HL)                                                V0303180
      WRITE (5,914)                                                     V0303190
      WRITE (5,907)                                                     V0303200
      WRITE (5,908)                                                     V0303210
      IF(NR-1) 270, 270, 275                                            V0303220
 270  LZ=NW-1                                                           V0303230
      GO TO 280                                                         V0303240
 275  LZ=NW                                                             V0303250
 280  DO 300 I=1,LZ                                                     V0303260
      LL(1)=PQ(I)                                                       V0303270
      DO 281 J=1,NV                                                     V0303280
      LL(2)=LL(1)                                                       V0303290
      LL(2)=LL(2)/10                                                    V0303300
      LL(3)=LL(2)                                                       V0303310
      LL(2)=LL(2)*10                                                    V0303320
      LY=NV-(J-1)                                                       V0303330
      N(LY)=LL(1)-LL(2)                                                 V0303340
 281  LL(1)=LL(3)                                                       V0303350
      LY=0                                                              V0303360
      KZ=2                                                              V0303370
      DO 285 J=1,NV                                                     V0303380
      IF(N(J)) 285, 285, 282                                            V0303390
 282  LY=LY+1                                                           V0303400
      LL(LY)=N(J)                                                       V0303410
      KZ=KZ+1                                                           V0303420
      DATA Q007HL/5HI1,  /
      BNEW(KZ)=(+Q007HL)                                                V0303440
 285  CONTINUE                                                          V0303450
      SQM=B(I)/A(I)                                                     V0303460
      NDF=A(I)                                                          V0303470
 300  WRITE (5,915)(LL(J), J=1,LY), NDF,B(I),SQM  
      IF(NR-1) 310, 310, 320                                            V0303490
 310  SQM=B(NW)/A(NW)                                                   V0303500
      NDF=A(NW)                                                         V0303510
      WRITE (5,911)NDF,B(NW),SQM                                        V0303520
      GO TO 330                                                         V0303530
 320  SQM=B(NY)/A(NY)                                                   V0303540
      NDF=A(NY)                                                         V0303550
      WRITE (5,913)NDF,B(NY),SQM                                        V0303560
 907  FORMAT(59H0SOURCE OF        DEGREES OF          SUMS OF          MV0303570
     1EAN)                                                              V0303580
 908  FORMAT(61H VARIATION         FREEDOM            SQUARES         SQV0303590
     1UARES/)                                                           V0303600
 911  FORMAT(18H RESIDUAL         I6,F22.5,F17.5)                       V0303610
 912  FORMAT(18H TOTAL            I6,F22.5)                             V0303620
 913  FORMAT(18H WITHIN REPLICATESI6,F22.5,F17.5)                       V0303630
 914  FORMAT(1H0)                                                       V0303640
  915 FORMAT(' 'I1,T22,I3,T35,F12.5,T52,F12.5)
 330  RETURN                                                            V0303650
      END                                                               V0303660
CFINAL     SUBROUTINE FINAL FOR BMD03V            AUGUST 17,1966        V0303680
C                                                                       *LMSC
C            CONVERTED FOR LMSC UNIVAC 1108 BY DEPT 84-72               *LMSC
C                   LEW BUTLER   DECEMBER 1967                          *LMSC
C                                                                       *LMSC
      SUBROUTINE FINAL                                                  V0303690
      DIMENSION X(1500,9),SCALE(9),AV(9),L(6),LL(6),N(6),M(6),MM(7),    V0303700
     1K5(7),KM(6),S(65,9,9),T(9,9),PQ(64),A(64),B(64),R(64,8),KV(9),    V0303710
     2KW(9),CON(8),NEWA(64),LCODE(64),LVA(64),BNEW(64),TT(9,9)          *LMSC
      COMMON /A/R                                                         V0303730
      COMMON /B/X      , SCALE  , AV     , L      , LL     , N            V0303740
      COMMON /C/M      , MM     , K5     , KM     , S      , T            V0303750
      COMMON /D/PQ     , NV     , NR     , NT     , NI     , NVM          V0303760
      COMMON /E/I5     , MARK   , JW     , Z      , IJK    , KK           V0303770
