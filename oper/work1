      SUBROUTINE XLOCAT                                                 V0308800
      DIMENSION X(1500,9),SCALE(9),AV(9),L(6),LL(6),N(6),M(6),MM(7),    V0308810
     1K5(7),KM(6),S(65,9,9),T(9,9),PQ(64),A(64),B(64),R(64,8),KV(9),    V0308820
     2KW(9),CON(8),NEWA(64),LCODE(64),LVA(64),BNEW(64)                  *LMSC
      COMMON /A/R                                                         V0308840
      COMMON /B/X      , SCALE  , AV     , L      , LL     , N            V0308850
      COMMON /C/M      , MM     , K5     , KM     , S      , T            V0308860
      COMMON /D/PQ     , NV     , NR     , NT     , NI     , NVM          V0308870
      COMMON /E/I5     , MARK   , JW     , Z      , IJK    , KK           V0308880
      COMMON /F/KZ     , NN     , ON     , NW     , A      , B            V0308890
      COMMON /G/KV     , KW     , NY     , NZ     , CON    , ONNN         V0308900
        DOUBLE PRECISION ON
      EQUIVALENCE (R(1),BNEW(1)),(R(65),NEWA),(R(129),LVA),             V0308910
     1(R(193),LCODE)                                                    V0308920
      DO 300 I=1,6                                                      V0308930
 300  N(I)=0                                                            V0308940
      KI=0                                                              V0308950
      K=1                                                               V0308960
      DO 302 I=I5,6                                                     V0308970
      KI=KI+1                                                           V0308980
      IF(M(I)) 301, 301, 302                                            V0308990
 301  N(K)=KI                                                           V0309000
      K=K+1                                                             V0309010
 302  CONTINUE                                                          V0309020
      DO 303 I=I5,6                                                     V0309030
 303  MM(I)=L(I)-1                                                      V0309040
      DO 305 I=1,NV                                                     V0309050
      K=I5+(I-1)                                                        V0309060
 305  K5(K)=I                                                           V0309070
      DO 310 I=1,JW                                                     V0309080
      DO 309 J=I5,6                                                     V0309090
      IF(N(I)-K5(J)) 309, 306, 309                                      V0309100
 306  K5(7)=K5(J)                                                       V0309110
      MM(7)=MM(J)                                                       V0309120
      DO 307 J2=J,6                                                     V0309130
      K5(J2)=K5(J2+1)                                                   V0309140
 307  MM(J2)=MM(J2+1)                                                   V0309150
      GO TO 310                                                         V0309160
 309  CONTINUE                                                          V0309170
 310  CONTINUE                                                          V0309180
      DO 320 J=1,NT                                                     V0309190
 320  AV(J)=0.0                                                         V0309200
 351  N(1)=0                                                            V0309210
 352  N(2)=0                                                            V0309220
 353  N(3)=0                                                            V0309230
 354  N(4)=0                                                            V0309240
 355  N(5)=0                                                            V0309250
 356  N(6)=0                                                            V0309260
 359  DO 360 J=I5,6                                                     V0309270
      KK=K5(J)                                                          V0309280
 360  KM(KK)=N(J)                                                       V0309290
      LX=1                                                              V0309300
      DO 361 J=1,NV                                                     V0309310
 361  LX=LX+KM(J)*LL(J)                                                 V0309320
      DO 200 J=1,NT                                                     V0309330
 200  AV(J)=AV(J)+X(LX,J)                                               V0309340
      IF(N(6)-MM(6)) 362, 363, 363                                      V0309350
 362  N(6)=N(6)+1                                                       V0309360
      GO TO 359                                                         V0309370
 363  IF(JW-1) 212, 364, 370                                            V0309380
 364  DO 210 I=1,NT                                                     V0309390
      DO 210 J=I,NT                                                     V0309400
 210  S(KZ,I,J)=S(KZ,I,J)+AV(I)*AV(J)                                   V0309410
      DO 211 I=1,NT                                                     V0309420
 211  AV(I)=0.0                                                         V0309430
 212  IF(NV-1) 70, 70, 370                                              V0309440
 370  IF(N(5)-MM(5)) 371, 372, 372                                      V0309450
 371  N(5)=N(5)+1                                                       V0309460
      GO TO 356                                                         V0309470
 372  IF(JW-2) 232, 373, 381                                            V0309480
 373  DO 230 I=1,NT                                                     V0309490
      DO 230 J=I,NT                                                     V0309500
 230  S(KZ,I,J)=S(KZ,I,J)+AV(I)*AV(J)                                   V0309510
      DO 231 I=1,NT                                                     V0309520
 231  AV(I)=0.0                                                         V0309530
 232  IF(NV-2) 70, 70, 381                                              V0309540
 381  IF(N(4)-MM(4)) 382, 383, 383                                      V0309550
