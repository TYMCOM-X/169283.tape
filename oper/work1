     1K5(7),KM(6),S(65,9,9),T(9,9),PQ(64),A(64),B(64),R(64,8),KV(9),    V0300080
     2KW(9),MD(6),CON(8),NEWA(64),LCODE(64),LVA(64),BNEW(64)            V0300090
      COMMON /A/R                                                         V0300100
      COMMON /B/X      , SCALE  , AV     , L      , LL     , N            V0300110
      COMMON /C/M      , MM     , K5     , KM     , S      , T            V0300120
      COMMON /D/PQ     , NV     , NR     , NT     , NI     , NVM          V0300130
      COMMON /E/I5     , MARK   , JW     , Z      , IJK    , KK           V0300140
      COMMON /F/KZ     , NN     , ON     , NW     , A      , B            V0300150
      COMMON /G/KV     , KW     , NY     , NZ     , CON    , ONNN         V0300160
      EQUIVALENCE (R(1),BNEW(1)),(R(65),NEWA),(R(129),LVA),             V0300170
     1(R(193),LCODE)                                                    V0300180
        DOUBLE PRECISION Q000HL,Q001HL,Q002HL,Q003HL,ON,ONR
C                                                                       V0300190
  902 FORMAT(46H1BMD03V - ANALYSIS OF COVARIANCE - VERSION OF ,         V0300200
     113HJULY 22, 1965/                                                 V0300210
     263H HEALTH SCIENCES COMPUTING FACILITY, UCLA--LMSC REVISION, DECEM*LMSC
     3 9HBER, 1967//)                                                   *LMSC
C                                                                       V0300230
      MTAPE=5                                                           V0300240
      REWIND 1                                                          V0300250
 4    READ (20,900)ON,PROB,NV,NR,NI,NVM,NJ,JW,(L(I),I=1,6), NTAPE,IJK    V0300260
      DATA Q000HL/6HPROBLM/                                             V0300270
      ONR=(+Q000HL)                                                     V0300280
      IF(ON.EQ.Q000HL)GO TO 6
      DATA Q001HL/6HFINISH/                                             V0300300
 5    IF(ON-(+Q001HL)) 990, 1000, 990                                   V0300310
 6    IF(NTAPE-2)8,990,8                                                V0300320
 8    CALL TPWD(NTAPE,MTAPE)                                            V0300330
 9    WRITE (5,902)                                                     V0300340
      WRITE (5,903)PROB                                                 V0300350
      IF(NV*(NV-7)) 6001,990,990                                        V0300360
 6001 IF(NI*(NI-9)) 6002,990,990                                        V0300370
 6002 IF((NJ+8)*(NJ-8)) 6003,990,990                                    V0300380
 6003 IF((NI+NJ)*(NI+NJ-9)) 6004,990,990                                V0300390
 6004 IF(NVM-64) 6005,6005,990                                          V0300400
 6005 IF(IJK*(IJK-6)) 6006,990,990                                      V0300410
 6006 KZ=0                                                              V0300420
      NTT=NI+1                                                          V0300430
      NI=NI+NJ                                                          V0300440
      NT=NTT+NJ                                                         V0300450
      IF(NR) 41, 41, 42                                                 V0300460
 41   NR=1                                                              V0300470
 42   WRITE (5,904)NV,NR,NI                                             V0300480
      WRITE (5,908)                                                     V0300490
      DO 7 I=1,NV                                                       V0300500
 7    WRITE (5,909)I,L(I)                                               V0300510
      IF(NVM)410,410,400                                                V0300520
 400  WRITE (5,912)                                                     V0300530
      WRITE (5,914)                                                     V0300540
      DATA Q002HL/6HTRNGEN/                                             V0300550
      ONR=(+Q002HL)                                                     V0300560
      DO 405 I=1,NVM                                                    V0300570
      READ (20,911)ON,NEWA(I),LCODE(I),LVA(I),BNEW(I)                    V0300580
      IF(ON.NE.ONR) GO TO 403                                           V0300590
  401 IF(LCODE(I)*(LCODE(I)-15))404,402,402                             V0300600
  402 WRITE (5,910)I                                                    V0300610
      GO TO 4035                                                        V0300620
  403 WRITE (5,906)ON                                                   V0300630
 4035 KZ=-9                                                             V0300640
      WRITE (5,916)                                                     V0300650
      GO TO 405                                                         V0300660
  404 WRITE (5,913)I,NEWA(I),LCODE(I),LVA(I),BNEW(I)                    V0300670
  405 CONTINUE                                                          V0300680
 410  DO 10 I=1,NT                                                      V0300690
 10   AV(I)=0.0                                                         V0300700
      IF(JW) 432, 432, 435                                              V0300710
 432  DO 433 I=1,NI                                                     V0300720
 433  CON(I)=0.0                                                        V0300730
      GO TO 413                                                         V0300740
 435  READ (20,901)ON,(CON(I), I=1,NI)                                   V0300750
      DATA Q003HL/6HCONSTS/                                             V0300760
      ONR=(+Q003HL)                                                     V0300770
      IF(ON.EQ.ONR) GO TO 413                                           V0300780
 411  WRITE (5,915)                                                     V0300790
      GO TO 1000                                                        V0300800
 413  CALL VFCHCK(IJK)                                                  V0300810
      IJK=IJK*12                                                        V0300820
      READ (20,907)(B(I), I=1,IJK)                                       V0300830
 11   NN=1                                                              V0300840
      DO 12 I=1,NV                                                      V0300850
 12   NN=NN*L(I)                                                        V0300860
      ON=NN                                                             V0300870
      ONR=NR                                                            V0300880
      ONN=ON*ONR                                                        V0300890
      ONNN=ONN+1.0                                                      V0300900
      NW=2**NV-1                                                        V0300910
      NY=NW+1                                                           V0300920
      NZ=NY+1                                                           V0300930
      DO 25 I=1,64                                                      V0300940
      DO 25 J=1,NT                                                      V0300950
      DO 25 K=1,NT                                                      V0300960
 25   S(I,J,K)=0.0                                                      V0300970
      IF(NR-1) 13, 13, 17                                               V0300980
 13   MARK=1                                                            V0300990
      DO 14 I=1,NN                                                      V0301000
      READ (MTAPE,B)(T(1,J),J=1,NTT)                                    V0301010
      IF(KZ) 14, 418, 418                                               V0301020
 418  IF(NVM)420,420,415                                                V0301030
 415  CALL TRANZ                                                        V0301040
 420  DO 75 J=1,NT                                                      V0301050
      X(I,J)=T(1,J)                                                     V0301060
 75   AV(J)=AV(J)+X(I,J)                                                V0301070
 14   CONTINUE                                                          V0301080
      IF(KZ) 4, 79, 79                                                  V0301090
 79   DO 15 J=1,NT                                                      V0301100
 15   AV(J)=AV(J)/ON                                                    V0301110
      DO 16 I=1,NN                                                      V0301120
      DO 16 J=1,NT                                                      V0301130
 16   X(I,J)=X(I,J)-AV(J)                                               V0301140
      DO 81 I=1,NN                                                      V0301150
      DO 81 J=1,NT                                                      V0301160
      DO 81 K=J,NT                                                      V0301170
 81   S(NZ,J,K)=S(NZ,J,K)+X(I,J)*X(I,K)                                 V0301180
      GO TO 26                                                          V0301190
 17   DO 18 I=1,NN                                                      V0301200
      DO 18 J=1,NT                                                      V0301210
 18   X(I,J)=0.0                                                        V0301220
       DO 20 I=1,NR
      MARK=I                                                            V0301240
      WRITE(5,923)
      DO 20 J=1,NN                                                      V0301250
      READ (20,924) (T(1,K),K=1,NTT)                                    V0301260
       WRITE(5,922) (T(1,K),K=1,NTT)
      IF(KZ) 20, 440, 440                                               V0301270
 440  IF(NVM) 90, 90, 430                                               V0301280
 430  CALL TRANZ                                                        V0301290
 90   DO 99 K=1,NT                                                      V0301300
      AV(K)=AV(K)+T(1,K)                                                V0301310
 99   X(J,K)=X(J,K)+T(1,K)                                              V0301320
      WRITE (1)(T(1,K), K=1,NT)                                         V0301330
 20   CONTINUE                                                          V0301340
      END FILE 1                                                        V0301350
      REWIND 1                                                          V0301360
      IF(KZ) 4, 445, 445                                                V0301370
 445  DO 21 I=1,NN                                                      V0301380
      DO 21 J=1,NT                                                      V0301390
 21   X(I,J)=X(I,J)/ONR                                                 V0301400
      DO 22 J=1,NT                                                      V0301410
 22   AV(J)=AV(J)/ONN                                                   V0301420
      DO 24 I=1,NR                                                      V0301430
      DO 24 J=1,NN                                                      V0301440
      READ (1)(T(1,K),K=1,NT)                                           V0301450
      DO 23 K=1,NT                                                      V0301460
      T(2,K)=T(1,K)-AV(K)                                               V0301470
 23   T(1,K)=T(1,K)-X(J,K)                                              V0301480
      DO 24 JA=1,NT                                                     V0301490
      DO 24 JB=JA,NT                                                    V0301500
      S(NY,JA,JB)=S(NY,JA,JB)+T(1,JA)*T(1,JB)                           V0301510
 24   S(NZ,JA,JB)=S(NZ,JA,JB)+T(2,JA)*T(2,JB)                           V0301520
      REWIND 1                                                          V0301530
      DO 38 J=1,NT                                                      V0301540
      AV(J)=AV(J)*ONR                                                   V0301550
      DO 38 I=1,NN                                                      V0301560
      X(I,J)=X(I,J)*ONR                                                 V0301570
 38   X(I,J)=X(I,J)-AV(J)                                               V0301580
 26   NVM=NV-1                                                          V0301590
      I5=(6-NV)+1                                                       V0301600
      DO 32 I=1,NVM                                                     V0301610
      LL(I)=1                                                           V0301620
      I1=I+1                                                            V0301630
      DO 32 J=I1,NV                                                     V0301640
 32   LL(I)=LL(I)*L(J)                                                  V0301650
      LL(NV)=1                                                          V0301660
      DO 40 I=1,NV                                                      V0301670
      I1=I-1                                                            V0301680
      I2=NV-I1                                                          V0301690
      I3=6-I1                                                           V0301700
 40   L(I3)=L(I2)                                                       V0301710
      KZ=0                                                              V0301720
      DO 45 I=1,64                                                      V0301730
 45   PQ(I)=0.0                                                         V0301740
      DO 231 IJ=1,NVM                                                   V0301750
      MARK=0                                                            V0301760
      IJK=IJ                                                            V0301770
      JW=NVM-(IJ-1)                                                     V0301780
      DO 51 I=I5,6                                                      V0301790
 51   M(I)=0                                                            V0301800
      J1=I5+(NVM-JW)                                                    V0301810
      DO 52 I=I5,J1                                                     V0301820
 52   M(I)=1                                                            V0301830
      Z=1.0                                                             V0301840
 70   CALL TABLE (M,I5,Z,MARK,IJK)                                      V0301850
      IF(MARK) 205, 205, 231                                            V0301860
 205  KZ=KZ+1                                                           V0301870
      CALL XLOCAT                                                       V0301880
      NDF=1                                                             V0301890
      MDF=1                                                             V0301900
      I6=I5+(IJ-1)                                                      V0301910
      DO 206 I=I5,I6                                                    V0301920
      NDF=NDF*MM(I)                                                     V0301930
 206  MDF=MDF*(MM(I)+1)                                                 V0301940
      FNDF=NDF                                                          V0301950
      DF=MDF                                                            V0301960
      FACT=DF/ONN                                                       V0301970
      DO 200 I=1,NT                                                     V0301980
      DO 200 J=1,NT                                                     V0301990
 200  S(KZ,I,J)=S(KZ,I,J)*FACT                                          V0302000
      A(KZ)=FNDF                                                        V0302010
      IF(NVM)2232,2232,2231                                             V0302020
 2231 DO 204 I=1,IJK                                                    V0302030
      I4=I5+(I-1)                                                       V0302040
      ROSE=K5(I4)                                                       V0302050
 204  PQ(KZ)=(PQ(KZ)+ROSE)*10.0                                         V0302060
      PQ(KZ)=PQ(KZ)/10.0                                                V0302070
      I7=I6-I5                                                          V0302080
      IF(I7-1) 70, 207, 207                                             V0302090
 207  DO 208 J=1,NT                                                     V0302100
      DO 208 K=J,NT                                                     V0302110
 208  T(J,K)=0.0                                                        V0302120
      DO 220 JK=1,I7                                                    V0302130
      ICE=0                                                             V0302140
      KL=JK                                                             V0302150
      DO 209 I=I5,6                                                     V0302160
 209  MD(I)=0                                                           V0302170
      I8=6-I7                                                           V0302180
      I9=I8+(JK-1)                                                      V0302190
      DO 210 I=I8,I9                                                    V0302200
 210  MD(I)=1                                                           V0302210
      Y=1.0                                                        V0302220
 211  CALL PARTS(MD,I8,Y,ICE,KL)                                        V0302230
      IF(ICE) 212, 212, 220                                             V0302240
 212  FISH=0.0                                                          V0302250
      KK=0                                                              V0302260
      DO 215 I=I8,6                                                     V0302270
      KK=KK+1                                                           V0302280
      IF(MD(I)) 215, 215, 213                                           V0302290
 213  I4=I5+(KK-1)                                                      V0302300
      ROSE=K5(I4)                                                       V0302310
      FISH=(FISH+ROSE)*10.0                                             V0302320
 215  CONTINUE                                                          V0302330
      FISH=FISH/10.0                                                    V0302340
      DO 217 I=1, 64                                                    V0302350
      IF(PQ(I)-FISH) 217, 216, 211                                      V0302360
 216  DO 222 J=1,NT                                                     V0302370
      DO 222 K=J,NT                                                     V0302380
 222  T(J,K)=T(J,K)+S(I,J,K)                                            V0302390
      GO TO 211                                                         V0302400
 217  CONTINUE                                                          V0302410
 220  CONTINUE                                                          V0302420
      DO 223 J=1,NT                                                     V0302430
      DO 223 K=J,NT                                                     V0302440
 223  S(KZ,J,K)=S(KZ,J,K)-T(J,K)                                        V0302450
      GO TO 70                                                          V0302460
 231  CONTINUE                                                          V0302470
 2232 DO 236 I=1,KZ                                                     V0302480
      DO 236 J=1,NT                                                     V0302490
      DO 236 K=J,NT                                                     V0302500
 236  S(NW,J,K)=S(NW,J,K)+S(I,J,K)                                      V0302510
      IF(NR-1) 238, 238, 250                                            V0302520
 238  DO 237 J=1,NT                                                     V0302530
      DO 237 K=J,NT                                                     V0302540
 237  S(NW,J,K)=S(NZ,J,K)-S(NW,J,K)                                     V0302550
      GO TO 360                                                         V0302560
 250  DO 255 I=1,NT                                                     V0302570
      DO 255 J=I,NT                                                     V0302580
 255  S(NW,I,J)=S(NZ,I,J)-S(NY,I,J)-S(NW,I,J)                           V0302590
 360  CALL FINAL                                                        V0302600
      CALL ANOVA                                                        V0302610
      GO TO 4                                                           V0302620
 900  FORMAT(A6,A4,I2,I3,4I2,6I3,27X,I2,I2)                             V0302630
 901  FORMAT(A6,9F6.0)                                                  V0302640
