.BLANK 1
.RIGHT MARGIN 70
<LABELED-EXPRESSION>::=<LABEL-LIST><BLOCK>/
.INDENT 23
<LABEL-LIST><COMPOUNDEXPRESSION>/
.INDENT 23
<LABEL-LIST><CONDITIONALEXPRESSION>/
.INDENT 23
<LABEL-LIST><LOOPEXPRESSION>/
.INDENT 23
<LABEL-LIST><CHOICEEXPRESSION>
.BLANK 1
<LABEL-LIST>::=<LABEL>:/<LABEL>:<LABEL LIST>
.RIGHT MARGIN 60
.BLANK 1
