TITLE CLOZE.MAC - C. NEUSTRUP - MAR. 13, 1973
; CLOSE FILES WITHOUT DEALLOCATING THE SPACE UNUSED
        INTERN CLOZE
        ENTRY   CLOZE

        OPDEF   FCALLI [35B8]

CLOZE:  JFCL
        MOVEM   0,SAVE
        MOVEI   0,24
        FCALLI  0,1B33
        MOVEI   0,30
        FCALLI  0,1B33
        MOVE    0,SAVE
        JRA     16,(16)

SAVE:   BLOCK   1

        END
