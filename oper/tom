BEGIN
  INTEGER N,F;
 IN: F:=1;
    INPUT(0,"TTY");
     PRINT (" HI YOU GUYS");
      READ (F);
 IF N<0 THEN GO TO OUT;
 RECUR: IF N>= 1 THEN
        BEGIN
           F:= F*N;
           N:= N-1;
           GO TO RECUR;
         END
                ELSE
         BEGIN
     PRINT (F);
        GO TO IN;
   END;
 OUT:END
