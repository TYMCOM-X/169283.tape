;MACRO DEFINITIONS

DEFINE  PRLIN1(B)
<       XLIST
        JSP     A,PRLIN
        SIXBIT  +B+
        LIST
>
DEFINE  PR1(B)
<       XLIST
        JSP     A,PR
        SIXBIT  +B+
        LIST
>
DEFINE  EMTAPE(B)
<       XLIST
        MTAPE   TAP,B
        MTAPE   TAP,0
        LIST
>
