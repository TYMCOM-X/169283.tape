LOCAL B(50),I,J
GLOBAL G:,ERR:

G: MSG('$STARTING ')
\\->G

I_1
J_1
LOOP DO
B(I) _ BLOCK(4,14,ERR)
I _ I+4
B(I) _ BLOCK(1,51,ERR)
INC I
B(I) _ BLOCK(4,64,ERR)
I _ I+4
RELEASE(4,B(J),ERR); B(J) _ -B(J)
J _ J+4
INC J
RELEASE(4,B(J),ERR); B(J) _ - B(J)
END
ERR: MSG('OUT OF CORE$')
