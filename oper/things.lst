  		            SUBROUTINE FONLY(FNUM,IWIDTH,IDEC,IARRAY,IFILL)
  		      C * THIS ROUTINE FORMATS INTO AN ARRAY A FLOATING POINT NUMBER
  		            INTEGER IARRAY(2)
  		            FWHOLE=IFIX(FNUM+SIGN(0.0000001,FNUM))
  		            FRAC=ABS(FNUM-FWHOLE)*10.**IDEC+0.5
