  		      C*     PLOT-10/DECISION MAKER RELEASE 1.0 - VERSION 1
  		      C*               MODIFICATION LEVEL 0
  		      C****************************************************************
  		            SUBROUTINE DTADD(IN,NUM,ITYPE,IOUT)
  		            I = IN/100
  		            J = IN - (I*100) + NUM
  		      C
  		            IF(J.GT.0 .AND. J.LE.ITYPE) GO TO 20
  		            IF(J.LT.0) GO TO 10
  		      C
  		            I =  I + J/ITYPE
  		            J =  MOD(J,ITYPE)
  		            IF(J.NE.0) GO TO 20
  		            I = I -1
  		            J = ITYPE
  		            GO TO 20
  		      C
  		      10    CONTINUE
  		            I = I - 1 + J / ITYPE
  		            J = MOD(IABS(J),ITYPE)
  		            IF(J.EQ.0) J = 1
  		            J = ITYPE - J
  		      C
  		      20    CONTINUE
  		            IOUT = I * 100 + J
  		            RETURN
  		            END

