      EQUIVALENCE (R(1),BNEW(1)),(R(65),NEWA(1)),(R(129),LVA(1))
     1,(R(193),LCODE(1))
C
  902 FORMAT('1BMD03V - ANALYSIS OF COVARIANCE - REVISED ',
     116HJANUARY 29, 1970/
