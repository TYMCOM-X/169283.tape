      Q=ICLY(K)+LV-IH
      DO 462 LK=1,MK
462   X9(LK)=X9(LK)+(HV*((1.+CLR(K)/12.)**(JJ/30.))-HV)*(VL(LK)**
     1(Q-SEN))
      LV=LV+1
      HV=HV*((1.+CLR(K)/12.)**(JJ/30.))
      LO=LO-JJ
      IF(LO)235,235,38
38    IF(LO-360)  39,39,40
39    JJ=LO
      GO TO 37
40    JJ=360
      GO TO 37
235   IF(ICL-K)35,35,41
41    LO=LA-360.*(ICLY(K+1)-ICLY(K))+IPFU(K+1)-IPFU(K)
      LA=LO
35    CONTINUE
2     IF(IR.EQ.0)   GO TO 2013
      DO 20 L=1,IR
      TYPE  6708,I,L
6708  FORMAT(' BASIC TERM RENT DATA CLOSING ',I2,'   RENTAL TYPE',I2/
     1  ' ?',$)
        ACCEPT 4044,MO,IRY,IRDD,IRD,((RR(K),RRI(K),IRRP(K),IRRT(K),
     1  ICLY(K)),K=1,MO),EQ,REQ
      DO 2011 K=1,MO
      CE(K)='B'
      IF((RR(K).GT.1).AND.(RR(K).LE.2))   GO TO 21
      GO TO 2011
21      TYPE 4679
        ACCEPT 4054,CE(K),PFR,PFB,IPFT
4054    FORMAT(A1,4G)
      S=IRRP(K)
      S=PFR/S
      W=1.+S
      IF(CE(K).EQ.B)   GO TO 22
      TT=S*(W**(IPFT-1)-PFB)/((W**IPFT)-1)
      GO TO 23
22    TT=S*PFB+(1-PFB)*S*(W**IPFT)/((W**IPFT)-1)
23    RR(K)=AS(I)*TT
2011  CONTINUE
      Q=IRY+1-IRD/360.
      Q1=Q
      LO=IRY+1-IH
      V=IRRP(1)
      V=IRDD/(360./V)
      DO 24 K=1,MO
      IF(RR(K).LE.0)  CALL ZA
      IF(RR(K).LE.0)   GO TO 24
      IF(RR(K).GE.1)   GO TO 25
      RR(K)=RR(K)*AS(I)
25    LV=IRRT(K)
      QQ=IRRP(K)
      QQ=1./QQ
      DO 26 JJ=1,LV
      Q=Q+QQ*V
      LO=Q-IH+.99999
      V=1
      LD=Q-Q1
      RI(LO)=RI(LO)+RR(K)*(1.+LD*RRI(K))
      DO 872 LK=1,MK
872   X(LK,1)=X(LK,1)+RR(K)*(1.+LD*RRI(K))*(VL(LK)**(Q-SEN))*(1-OO)
26    CONTINUE
      Q1=Q
24    CONTINUE
      VOST=Q
      TOST=VOST
      DO 851 K=1,MO
      IF(((CE(K).EQ.A).OR.(IRDD.LE.0)).AND.(TOST.EQ.VOST))  TOST=VOST+
     9QQ
851   CE(K)=B
      IF(I.EQ.1)  MAS=TOST-IH+.99999
      IF((TOST+1-IH).GT.MAS)  MAS=TOST-IH+.999999
20    CONTINUE
      IR=0
2013  IF(IEW.LE.0)   GO TO 2900
      DO 200 L=1,IEW
      TYPE  6820,I,L
6820  FORMAT (' RENEWAL RENT DATA CLSNG',I2,'  RENTAL TYPE',I2,/' ?',$)
        ACCEPT 4044,IRW,IRY,IRDD,IRD,((RR(K),RRI(K),IRRP(K),IRRT(K))
     1  ,K=1,IRW)
      IF(L.EQ.1)  MAL=IRW
      IF(IRW.GT.MAL)  MAL=IRW
      DO 1900 K=1,IRW
      CE(K)='B'
      IF((RR(K).GT.1).AND.(RR(K).LE.2))   GO TO 221
      GO TO 1900
221     TYPE 4679
4679    FORMAT(' ?',$)
        ACCEPT 4044,CE(K),PFR,PFB,IPFT
      S=IRRP(K)
      S=PFR/S
      W=1+S
      IF(CE(K).EQ.B)   GO TO 222
      TT=S*(W**(IPFT-1)-PFB)/((W**IPFT)-1)
      GO TO 223
222   TT=S*PFB+(1-PFB)*S*(W**IPFT)/((W**IPFT)-1)
