      COMMON MLO,OO,OC,CC,CO,IH,IHD,AS(6),AL(6),MAL
      COMMON ZOTT(6),ZOP(6),JOS,VOX(6),C(10),SEN,X9(7)
      COMMON RI(50),X(7,11),D(75),BB(75),B1(75),BS(75),DS(75)
      COMMON BS1(75),FOT(6),EQI(26),T(75),RE(50),X7(7,11)
      COMMON X77(7,11),X15(7,11),X13(11),X17(6),X16(7)
      COMMON X14(7,11),TPF(6),PFC(6),IPFU(25),RRI(6),X18(75),X181(75
     9)
      COMMON PI(50),PP(50),PB(50),X3(7,11),X4(7,11),AD(10),MAS,IRV,MK,
     1VL(50)
      COMMON VM(50),RWI(75),RWE(75),JOP,PFA(6),CA,CAA,LEO,IRRP(6),
     1IRRT(6)
      COMMON RR(6),CE(6),EQ,REQ,Q,CLA(25),CLR(25),ICLY(25),NA(6),NB(6)
      COMMON AP(6),BL(6),SA(6),CD(6),CV(6),CG(6),DZ(6)
      COMMON DOS(6),IPF,K,IR,I,E(75),TOST,D1(75)
      COMMON PB2(75),PP2(75),PI2(75),EM(75),EQS(75),EQE(75),SIM1(6,11)
        DIMENSION SOX(200)
      DATA A,B,ALPC,ALPD,ALPE,SOYD,SL,DB,NC/'A   ',
     1 'B   ','C   ','D   ','E   ','SOYD','SL  ','DB  ',
     1 'NC  '/
      TYPE  700
700   FORMAT(' INITIAL INPUTS'/' ?',$)
        ACCEPT 4044,MLO,OO,OC,ESAC,RTN,CC,CO,IH,IHD,TOP,AOO,AOC,IRV
      MK=IRV+2
      IF(IRV.LE.0)   GO TO 2099
      TYPE  702
702   FORMAT(' INPUT OTHER ANNUAL DISCOUNT RATES'/' ?',$)
        ACCEPT 4044,(VM(K),K=3,MK)
2099  DO 2351 K=1,MK
      IF(K.EQ.1)  VL(K)=1./(1.+CC)
      IF(K.EQ.2)  VL(K)=1./(1+CO)
      IF(K.GT.2)  VL(K)=1./(1.+VM(K))
2351  CONTINUE
      SEN=IH+1-IHD/360.
      JOS=0
      DO100 I=1,MLO
      TYPE  1,I
1     FORMAT(' GROUP DATA CLOSING ',I2,/' ?',$)
        ACCEPT 4044,AS(I),AL(I),IASY,IASB,IR,IEX,IEW,ICL,ICDD,IEQ,
     1  IPF,IABS
      ZOTT(I)=IASY+1-IASD/360.
      ZOP(I)=ZOTT(I)
      IF(ICL.GT.0)  ZOP(I)=ZOP(I)+ICDD/360.
      IF(ICL.LE.0)   GO TO 2
      TYPE  6701,I
6701  FORMAT(' CONSTRUCTION LOAN CLOSING',I2/' ?',$)
        ACCEPT 4044,((CLA(K),CLR(K),ICLY(K),IPFU(K)),K=1,ICL)
      LO=ICDD
      LA=ICDD
      DO 35 K=1,ICL
      JJ=IPFU(K)
      IF(LO.GT.IPFU(K))   GO TO 36
      JJ=LO
36    LV=ICLY(K)-IH+1
      HV=CLA(K)
      VOX(I)=VOX(I)+HV*((1+CLR(K)/12.)**(LO/30.))-HV
