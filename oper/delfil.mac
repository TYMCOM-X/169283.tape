SIXAN:	SETZ	T1,
	MOVE	T2,[POINT 6,T1]
	PUSHJ	P,SSP
	JRST	.+2
SIXANL:	PUSHJ	P,TTI
	CAIG	T,"Z"
	CAIGE	T,"0"
