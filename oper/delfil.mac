	PUSH	P,T
	MOVEI	T,"-"		;PUT OUT MINUS SIGN
	PUSHJ	P,TYO
	POP	P,T		;RESTORE WORD
	MOVMS	T		;POSITIVE VALUE
RDXPR1:	IDIVI	T,(T2)
	HRLM	T1,(P)
	SKIPE	T
	PUSHJ	P,RDXPR1
	HLRZ	T,(P)
	ADDI	T,"0"
TYO:	SOSG	OBUF+2
	OUTPUT	TTY,
	IDPB	T,OBUF+1
	POPJ	P,

COMMA:	SKIPA	T,[","]
