         TV$8NDWLDO ,'8NDWLDO ','$GEWDO',
         TV$8BGDOWL ,'8BGDOWL ','$GBWL ',
         TV$8DOWL   ,'8DOWL   ','$GDOWL',
         TV$8NDDOWL ,'8NDDOWL ','$GEDOW',
         TV$8BGACC  ,'8BGACC  ','$GBACC',
