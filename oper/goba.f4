      SUBROUTINE SINFUN
      A=-150.
      CALL MOVEA(0.,SIN(.01745*A)*(-125.)+225.)
      DO 100 I=1,300,10
      A=A+10.
 100      CALL DRAWA(A+150.,SIN(.01745*A)*(-125.)+225.)
      RETURN
      END
      SUBROUTINE JAGFUN
      CALL MOVEA(0.,50.)
      A=50.
      S=-1.
      DO 100 I=10,300,10
      X=I
      CALL DRAWA(X,A+S*15.)
      A=A+5.
 100      S=-1*S
      RETURN
      END
