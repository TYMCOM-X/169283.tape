TITLE	DIAGS FOR COBOL 5(107)	
SUBTTL	DIAGNOSTIC MESSAGES	AL BLACKINGTON/CAM

;COPYRIGHT 1970,1971,1972, DIGITAL EQUIPMENT CORP, MAYNARD, MASS

TWOSEG
RELOC	400000

	XALL

	ENTRY	 SETDN
	INTERNAL SETFAK
	EXTERNAL FAKERA

;THIS ROUTINE SETS UP "TE" TO POINT TO THE MESSAGE
;	WHOSE NUMBER IS IN "TB"

SETDN:	LDB	TE,TBNUM	;GET THE NUMBER

	CAIG	TE,LASTLO	;IS IT ONE IN LOWER SECTION?
	JRST	SETDN1		;YES

	CAIG	TE,LASTHI	;NO--IS IT PRESENT?
	CAIGE	TE,FRSTB2
	JRST	SETDN2		;NO--USE FAKE ONE

	SKIPA	TE,TABLE2-FRSTB2(TE);YES--GET POINTER
SETDN1:	MOVE	TE,TABLE1(TE)
	JUMPE	TE,SETDN2	;USE ERROR IF BAD NUMBER
	POPJ	PP,		;RETURN

;FAKE DIAGNOSTIC REQUIRED

SETDN2:	LDB	TE,TBNUM	;CONVERT DIAG # TO DECIMAL
	IDIVI	TE,12
	LSHC	TD,-7
	IDIVI	TE,12
	LSHC	TD,-7
	MOVEI	TD,"0"(TE)
	LSHC	TD,16
	ADDI	TD,14060

	DPB	TD,[POINT 21,FAKERA+3,27]; STASH # IN MESSAGE

	LDB	TE,TBFAZE
	DPB	TE,[POINT 4,FAKERA+6,13]

	MOVE	TE,[POINT 7,FAKERA,6]
	POPJ	PP,


SETFAK:	XWD	.+1,7
	ASCIZ	/FDIAGNOSTIC NO. XXX FROM PHASE A/
;IF THE FIRST CHARACTER OF A DIAGNOSTIC MESSAGE IS "A", THAT
;	MESSAGE WILL APPEAR AT THE END OF THE SOURCE LISTING.
;IF THE CHARACTER IS "F", THE DIAG IS FATAL.
;IF ANY OTHER CHARACTER (SPECIFICALLY "W"),
;	 THE DIAG IS A WARNING IMBEDDED IN SOURCE LISTING.

	DEFINE ERROR (X,Y),<
	XLIST
ERA'X:	ASCIZ $'Y'$
	IFG ^D'X-LASTLO,<
	IFL ^D'X-FRSTB2,<LASTLO==^D'X>>
	IFG ^D'X-LASTHI,<LASTHI==^D'X>
	LIST
	>

	INTERNAL FRSTHI
	LASTLO==0
	LASTHI==0
	FRSTHI==^D500
	FRSTB2==FRSTHI-4



