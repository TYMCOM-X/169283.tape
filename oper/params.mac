DEFINE	WERRS(PHASE,N1,N2)<
ERRNO.==N1
REPEAT N2'-'N1'+1,<
	WERR	PHASE,ERRNO.,\ERRNO.
ERRNO.==ERRNO.+1
	>
	>

DEFINE	WERR(PHASE,N,NUMBER)<
PHASE'E'NUMBER'.==N	;DIAGNOSTIC NUMBER
	>

