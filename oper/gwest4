C   TRUE INTEREST RTE
C **** A IS UNPAID BALANCE*R IS ANNUAL % RATE**MP IS MONTH PAYMENT*N=TE
C RM**
      SUBROUTINE SUB1(R,A,MP,N,IERROR)
      REAL R,A,MP
      TPY=MP*N
      NY=12
      IF (N.EQ.1) GO TO 3956
      IF (TPY.GT.A .OR. TPY.EQ.A) GO TO 3952
 9021 FORMAT (' ',' BAD NO',F8.2,F6.2,' ',$)
998     FORMAT(F,F,' ',$)
3912    WRITE (5,998)TPY,A
      WRITE( 5, 9022)R
      IERROR=1
      GO TO 3982
 9022 FORMAT ('+','REDUCE ADD ON $ RATE',F8.4,' ',$)
 3952 R=0
      D=100
 3966 R1=R/(100*NY)
      Q=1+R1
      IF (N*ALOG(Q)/ALOG(10.).LE.75) GO TO 3970
      P1= A*R1
      GO TO 3974
 3970 IF (Q.GT.1)GO TO 3972
      P1=A/N
      GO TO 3974
 3972 P1=A*Q**N*R1/(Q**N-1)
 3974 CONTINUE
      IF (MP.EQ.P1) GO TO 3962
      IF (MP.GT.P1) GO TO 3964
      R=R-D
      GO TO 3954
 3964 R=R+D
 3954 D=D/2
      IF (D.LT.0.0001) GO TO 3962
      GO TO 3966
 3956 R=(MP/A-1)*NY
      R=100*R
 3962 R=0.01*(0.5+100*R)
      IF (R.LT. 21.5) GO TO 3960
C  *IO*  DISPLAY 'RATE OVER 21.5%'
      GO TO 3912
 9020 FORMAT ('+','TRUE ANNUAL INT%',F8.4,' ',$)
 3960 WRITE( 5, 9020)R
 3982 RETURN
      END
C IDAYNO(NUM% RETURNS A DAY NUMBER GIVEN NUM=YYMMDD, A DATE
C IN INTEGER FORM. ROUNTINE IS GOOD FOR DATES BETWEEN MAR 1,1900
C THRU FEB 28,2100. DAY NUMBER FOR MAR 1,1900 IS 1 .
      INTEGER FUNCTION IDAYNO(NUM)
      IY=NUM/10000
      ID=NUM-1000*IY
      M=ID/100
      I=(12-M)/10
      IDAYNO=ID-100*M+(306*(M-3+12*I)+5)/10+36525*(IY-I)/100
      RETURN
      END
C IDATE (NUM) RETURNS DATE IN INTEGER FORM YYMMDD WHEN GIVEN
C A DAY NUMBER (NUM) AS CALCULATED BY FUNCTION IDAYNO.
      INTEGER FUNCTION IDATE (NUM)
      ID=100*NUM-10
      IY=ID/36525
      ID=NUM-36525*IY/100
      M=(10*ID-5)/306
      ID=ID-(306*M+5)/10
      I=M/10
      IDATE=10000*(IY+I)+100*(M+3-12*I)+ID
      RETURN
      END
      SUBROUTINE SUB2(KDAT,IY,IM,ID)
C USE TO SEPARATE IDAT INTO YR MO DAY WHERE IDATE=DATE=YYMMDD
      IY=KDAT/10**4
      IM=(KDAT-IY*10**4)/10**2
      ID= KDAT-IY*10**4-IM*10**2
      RETURN
      END
C USE TO ASSEMBLE DATE FROM IMONTHS,IDAYS,IYEARS
      SUBROUTINE SUB3(NEWDAT,IYR1,IMO1,IDY1)
      IYR1=IYR1*10**4
      IMO1=IMO1*10**2
      NEWDAT=IYR1+IMO1+IDY1
      RETURN
      END
      SUBROUTINE SUB4 (MDYDATE,IY,IM,ID)
C USED TO COMBINE DATE INTO MONTH DAY YEAR FORMAT
      IM=IM*10**4
      ID=ID*10**2
      IY=IY
      MDYDATE=IM+ID+IY
      RETURN
      END
