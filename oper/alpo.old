MODULE ALPO(DOLLAR, ENTRIES=($COUTA), FSAVE,TIMING,
                        TIMER=EXTERNAL(SIX12) )=
BEGIN

REQUIRE KDEFS.BLI[7,107355];
REQUIRE CDEFS.BLI[7,107355];
REQUIRE ADEFS.BLI[7,107355];

UNDECLARE $COUTA;


GLOBAL ROUTINE $COUTA(BUF)=
  BEGIN
  LOCAL KFCB$ FCB;
  FCB_.CFLCB$[CV$ALPHAOUT];
  FCB[K$DCNT]_AV$SZTOK;
  FCB[K$DADR]_.BUF;
  $KWRITE(FCB);
  CACTI$[CV$ALPHAOUT]_.CACTI$[CV$ALPHAOUT]+1;
  RETURN AV$SZTOK
  END;

%THIS ROUTINE WILL EVENTUALLY BE EXPANDED TO ALLOW FOR
 TRANSFER OF TOKENS TO CORE AS AN ALTERNATIVE TO DISK WHEN
 "FILE" SIZE PERMITS.
%

END
ELUDOM
