	SUBROUTINE LEDGER
	IMPLICIT INTEGER(A-Z)
	REAL TOTAL1
	COMMON /FORM/LINES,PAGE,MAX,INVNUM,CNTR,DISTRC,TXTOT
	COMMON/IDATE/IDATE(3),MONTHS(12)
	COMMON /PRT/ LPT,NAME(70),TOTAL,TOT(300),TAB(300,7),SLSM,SUBTOT
	COMMON /JRN/RPT1,RPT2
100	FORMAT(1X2I2,'-',I4,'-',I1,'33',3XI4,I2,'33',3X6A5,3XF9.2)
	TOTAL1=FLOAT(TOTAL)/100.
	INUM=INVNUM-1
	REGN=DISTRC/10
	WRITE(RPT1,100) IDATE(1),IDATE(3),INUM,REGN,NAME(1),DISTRC,
     1 (NAME(J),J=2,7),TOTAL1
	RETURN
	END
