      SUBROUTINE SORTX (IRAY,ID1,NUM,IW)
        COMMON/FTBL/NAMES(40),LV(10),LU(-9/40)
C**** THIS ROUTINE WILL SORT ARRAY IRAY ALPHANUMERICALLY ON TH IW WORD
C**** THE ID1 WORD FRAME IN THE ARRY, IT IS USED TO SORT THE ADDITIONS
C**** TO THE CITYS AND CARRIERS ARRAYS THAT THE USER HAD ADDED TO THE FI
C**** NUM = NO. FRAMES IN ARRAY
      DIMENSION IRAY(ID1,NUM)
      IF(NUM.LE.1) RETURN
      N=NUM
      M=N
   10 M=M/2
      IF(M) 20,40
   20 K=N-M
      J=1
   25 I =J
   30 L=I + M
      IF(IRAY(IW,I)-IRAY(IW,L)) 35,35
      DO 33 II=1,ID1
      ITEM=IRAY(II,I)
      IRAY(II,I)=IRAY(II,L)
   33 IRAY(II,L)=ITEM
      I=I-M
      IF(I .GT. 0) GO TO 30
   35 J=J+1
      IF(J .GT. K) GO TO 10
      GO TO 25
   40 RETURN
      END
CIR   FN5     MERG,MERG
