      SUBROUTINE RSTTAB(ITAB,ITBTBL)
      DIMENSION ITBTBL(1)
      COMMON /TKTRNX/ KBAUDR, KERROR, KGRAFL, KHOMEY, KKMODE,
     1 KHORSZ, KVERSZ, KITALC, KSIZEF, KLMRGN, KRMRGN,
