%STR.COMP(S1,S2)
LOCAL P1,P2,A,B

P1 _ CHPT(S1,-1)
P2 _ CHPT(S2,-1)
WHILE (A_NCHV(P1))=(B_NCHV(P2))#E.O.LIT THEN NULL
RETURN IF A=B THEN 1 ELSE 0
END STR.COMP
