 ORIF NTRANS#TRANS THEN STERM.TRANS
 ORIF NPROD#PROD THEN STERM.PROD
 ORIF NMODEL#MODEL THEN STERM.MODEL
 ORIF NBILL#BILL THEN STERM.BILL
