 GET.VALUES(GET.REC(IN.FN))
 EIF NTRANS#TRANS THEN STERM.TRANS
 ORIF NPROD#PROD THEN STERM.PROD
 ORIF NMODEL#MODEL THEN STERM.MODEL
 ORIF NBILL#BILL THEN STERM.BILL
 ORIF NSTATUS(1)#STATUS(1) THEN TERM.STATUS
 ELSE MOV.VALUES
