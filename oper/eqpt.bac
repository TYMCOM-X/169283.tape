MOV.VALUES
PR.DIST

LOOP DO
BUG: NULL
 LEVEL _ FALSE
 ADD(STATUS.TOT,QCRP)
 GET.VALUES(GET.REC(IN.FN))
 EIF NDIST#DIST THEN TERM.DIST
 ORIF NPROD#PROD THEN TERM.PROD
 ORIF NMODEL#MODEL THEN TERM.MODEL
 ORIF NTRANS#TRANS THEN TERM.TRANS
 ORIF NBILL#BILL THEN TERM.BILL
 ORIF NSTATUS(1)#STATUS(1) THEN TERM.STATUS
 ELSE [MOV.VALUES;PR.STATUS]
END     !END LOOP

EOF.D.PROD: NULL
 CLOSE(IN.FN)
 MOV.VALUES
 TOT.DIST
 PR.GRAND
 MSG('DETAILED STATUS REPORT OF EQUIPMENT FINISHED.$')
 MSG('THIS REPORT CAN BE PRINTED FROM THE FILE "EQPREP".$')
 MSG('PROGRAM TERMINATED.$')
 CLOSE(OUT.FN);EXIT             !************TEMPORARY***********
